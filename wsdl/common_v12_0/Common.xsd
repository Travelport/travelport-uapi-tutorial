tRemark">        <xs:annotation>            <xs:documentation>A textual remark container to hold any printable text. (max 512 chars)</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:simpleContent>                <xs:extension base="xs:string">                    <xs:attribute name="Key" type="typeRef" use="required"/>                </xs:extension>            </xs:simpleContent>        </xs:complexType>    </xs:element>    <xs:element name="GeneralRemark">        <xs:annotation>            <xs:documentation>A textual remark container to hold any printable text. (max 512 chars)</xs:documentation>        </xs:annotation>        <xs:complexType>           <xs:sequence>		    <xs:element name="RemarkData" type="xs:string">		     <xs:annotation>		      <xs:documentation>Actual remarks data.</xs:documentation>		     </xs:annotation>		    </xs:element>		    <xs:element name="BookingTravelerRef" type="typeRef" minOccurs="0" maxOccurs="unbounded">		     <xs:annotation>		      <xs:documentation>Reference to Booking Traveler.</xs:documentation>		     </xs:annotation>		    </xs:element>		   </xs:sequence>		   <xs:attribute name="Key" type="typeRef" use="optional"/>		   <xs:attribute name="Category" use="optional">		    <xs:annotation>		     <xs:documentation>A category to group and organize the various remarks. This is not required, but it is recommended.</xs:documentation>		    </xs:annotation>		    <xs:simpleType>		     <xs:restriction base="xs:string">		      <xs:maxLength value="10"/>		     </xs:restriction>		    </xs:simpleType>		   </xs:attribute>		   <xs:attribute name="TypeInGds" type="typeGdsRemark" use="optional"/>		   <xs:attribute name="SupplierType" type="typeProduct" use="optional">		    <xs:annotation>		     <xs:documentation>The type of product this reservation is relative to</xs:documentation>		    </xs:annotation>		   </xs:attribute>		   <xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">		    <xs:annotation>		     <xs:documentation>Provider reservation reference key.</xs:documentation>		    </xs:annotation>		   </xs:attribute>		   <xs:attributeGroup ref="attrProviderSupplier"/>		   <xs:attribute name="Direction" type="typeDirection" use="optional">		    <xs:annotation>		     <xs:documentation>Direction Incoming or Outgoing of the GeneralRemark.</xs:documentation>		    </xs:annotation>		   </xs:attribute>		   <xs:attribute name="CreateDate" type="xs:dateTime" use="optional">		    <xs:annotation>		     <xs:documentation>The date and time that this GeneralRemark was created.</xs:documentation>		    </xs:annotation>		   </xs:attribute>		   <xs:attribute name="UseProviderNativeMode" type="xs:boolean" use="optional" default="false">		    <xs:annotation>		     <xs:documentation>Will be true when terminal process required, else false</xs:documentation>		    </xs:annotation>		   </xs:attribute>        </xs:complexType>    </xs:element>    <xs:element name="CommissionRemark">		<xs:annotation>			<xs:documentation>Identifies the agency commision remarks.				Specifically used for Worldspan.</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:choice>				<xs:element name="ProviderReservationLevel">					<xs:annotation>						<xs:documentation>Specify commission which is applicable to							PNR level.</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:attributeGroup ref="attrCommissionRemark" />					</xs:complexType>				</xs:element>				<xs:element name="PassengerTypeLevel" maxOccurs="4">					<xs:annotation>						<xs:documentation>Specify commission which is applicable to							per PTC level.</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:attribute name="TravelerType" type="typePTC" use="required" />						<xs:attributeGroup ref="attrCommissionRemark" />					</xs:complexType>				</xs:element>			</xs:choice>			<xs:attribute name="Key" type="typeRef" use="optional">				<xs:annotation>					<xs:documentation>Key to be used for internal processing.					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="ProviderReservationInfoRef" type="typeRef"				use="optional">				<xs:annotation>					<xs:documentation>Provider reservation reference key.					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="ProviderCode" type="typeProviderCode"				use="optional">				<xs:annotation>					<xs:documentation>Contains the Provider Code of the provider						for which this accounting remark is used</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>		<xs:attributeGroup name="attrCommissionRemark">		<xs:attribute name="Amount" type="typeMoney" use="optional">		<xs:annotation>			<xs:documentation>The monetary amount of the commission.			</xs:documentation>		</xs:annotation>		</xs:attribute>		<xs:attribute name="Percentage" type="typePercentageWithDecimal"			use="optional">			<xs:annotation>				<xs:documentation>The percent of the commission.				</xs:documentation>			</xs:annotation>		</xs:attribute>		<xs:attribute name="CommissionCap" type="typeMoney" use="optional">			<xs:annotation>				<xs:documentation>Commission cap for the Airline.				</xs:documentation>			</xs:annotation>		</xs:attribute>	</xs:attributeGroup>    <xs:simpleType name="typeDirection"> 		<xs:annotation>			<xs:documentation>Defines the Direction for Incoming or Outgoing</xs:documentation>		</xs:annotation> 		<xs:restriction base="xs:string">  			<xs:enumeration value="Incoming"/>  			<xs:enumeration value="Outgoing"/> 		</xs:restriction>	</xs:simpleType>    <xs:element name="AccountingRemark">        <xs:annotation>            <xs:documentation>An accounting remark container to hold any            printable text.</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:sequence>                <xs:element name="RemarkData" type="xs:string">                    <xs:annotation>                        <xs:documentation>Actual remarks data.</xs:documentation>                    </xs:annotation>                </xs:element>                <xs:element name="BookingTravelerRef" type="typeRef"                    minOccurs="0" maxOccurs="unbounded">                    <xs:annotation>                        <xs:documentation>Reference to Booking Traveler.</xs:documentation>                    </xs:annotation>                </xs:element>            </xs:sequence>            <xs:attribute name="Key" type="typeRef" use="optional" />            <xs:attribute name="Category" use="optional">                <xs:annotation>                    <xs:documentation>A category to group and organize the various                    remarks. This is not required, but it is recommended.</xs:documentation>                </xs:annotation>                <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:maxLength value="14" />                    </xs:restriction>                </xs:simpleType>            </xs:attribute>            <xs:attribute name="TypeInGds" type="typeGdsAccountingRemark"                use="optional" />            <xs:attribute name="ProviderReservationInfoRef" type="typeRef"                use="optional">                <xs:annotation>                    <xs:documentation>Provider reservation reference key.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="ProviderCode" type="typeProviderCode"                use="optional">                <xs:annotation>                    <xs:documentation>Contains the Provider Code of the provider                    for which this accounting remark is used</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="UseProviderNativeMode" type="xs:boolean"                use="optional" default="false">                <xs:annotation>                    <xs:documentation>Will be true when terminal process required,                    else false</xs:documentation>                </xs:annotation>            </xs:attribute>        </xs:complexType>    </xs:element>    <xs:element name="XMLRemark">        <xs:annotation>            <xs:documentation>A remark container to hold an XML document. (max 1024 chars) This will be encoded with xml encoding.</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:simpleContent>                <xs:extension base="xs:string">                    <xs:attribute name="Key" type="typeRef" use="optional"/>                    <xs:attribute name="Category" use="optional">                        <xs:annotation>                            <xs:documentation>A category to group and organize the various remarks. This is not required, but it is recommended.</xs:documentation>                        </xs:annotation>                        <xs:simpleType>                            <xs:restriction base="xs:string">                                <xs:maxLength value="10"/>                            </xs:restriction>                        </xs:simpleType>                    </xs:attribute>                </xs:extension>            </xs:simpleContent>        </xs:complexType>    </xs:element>    <xs:element name="HostToken">        <xs:annotation>            <xs:documentation>This is a host token. It contains some kind of payload we got from a host</xs:documentation>            <xs:documentation>that must be passed in on successive calls they know who you are as our system</xs:documentation>            <xs:documentation>does not maintain state. The format of this string isn't important as long as it</xs:documentation>            <xs:documentation>is not altered in any way between calls. Since a host token is only valid on</xs:documentation>            <xs:documentation>the host it is assocated with, there is also an attribute called Host so we know</xs:documentation>            <xs:documentation>how to route the command(s). You can have multiple active sessions between</xs:documentation>            <xs:documentation>one or more hosts</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:simpleContent>                <xs:extension base="xs:string">                    <xs:attribute name="Host" type="typeProviderCode">                        <xs:annotation>                            <xs:documentation>The host associated with this token</xs:documentation>                        </xs:annotation>                    </xs:attribute>                    <xs:attribute name="Key">                           <xs:annotation>                                   <xs:documentation>Unique identifier for this token - use this key when a single HostToken is shared by multiple elements. </xs:documentation>                           </xs:annotation>                    </xs:attribute>                </xs:extension>            </xs:simpleContent>        </xs:complexType>    </xs:element>    <xs:element name="HostTokenList">        <xs:annotation>            <xs:documentation>The shared object list of Host Tokens</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:sequence>                <xs:element ref="HostToken" minOccurs="1" maxOccurs="unbounded" />            </xs:sequence>        </xs:complexType>    </xs:element>    <xs:element name="GuaranteeType" type="typeGeneralText">        <xs:annotation>            <xs:documentation>A type of guarantee i.e </xs:documentation>        </xs:annotation>    </xs:element>    <xs:element name="AppliedProfile">        <xs:annotation>            <xs:documentation>A simple container to specify the profiles that were applied to a reservation.</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="TravelerID" type="xs:string">                <xs:annotation>                    <xs:documentation>The ID of the TravelerProfile that was applied</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="TravelerName" type="xs:string">                <xs:annotation>                    <xs:documentation>The name from the TravelerProfile that was applied</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="AccountID" type="xs:string">                <xs:annotation>                    <xs:documentation>The ID of the AccountProfile that was applied</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="AccountName" type="xs:string">                <xs:annotation>                    <xs:documentation>The name from the AccountProfile that was applied</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="ImmediateParentID" type="xs:string" use="optional">                <xs:annotation>                    <xs:documentation>The ID of the immediate parent that was applied</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="ImmediateParentName" type="xs:string" use="optional">                <xs:annotation>                    <xs:documentation>The name of the immediate parent that was applied</xs:documentation>                </xs:annotation>            </xs:attribute>        </xs:complexType>    </xs:element>    <!--*** Air Reference Elements ***-->    <xs:element name="AccountCode">        <xs:annotation>            <xs:documentation>Account Code is used to get Private Fares.If ProviderCode or SupplierCode is not specified,</xs:documentation>             <xs:documentation>it will be considered a default AccounCode to be sent to all the Providers or Suppliers. </xs:documentation>                           </xs:annotation>        <xs:complexType>            <xs:attribute name="Code" use="required">               <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:maxLength value="36"/>                    </xs:restriction>                </xs:simpleType>            </xs:attribute>            <xs:attributeGroup ref="attrProviderSupplier"/>                    </xs:complexType>    </xs:element>    <xs:element name="AirSearchParameters">        <xs:annotation>            <xs:documentation>Search Parameters</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="NoAdvancePurchase" type="xs:boolean" use="optional"/>            <xs:attribute name="RefundableFares" type="xs:boolean" use="optional"/>            <xs:attribute name="NonPenaltyFares" type="xs:boolean" use="optional"/>            <xs:attribute name="UnRestrictedFares" type="xs:boolean" use="optional"/>        </xs:complexType>    </xs:element>    <xs:element name="Commission">        <xs:annotation>            <xs:documentation>Identifies the agency commission</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="Key" type="typeRef" use="optional"/>            <xs:attribute name="Level" type="typeCommissionLevel" use="required">                <xs:annotation>                    <xs:documentation>The commission percentage level.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="Type" type="typeCommissionType" use="required">                <xs:annotation>                    <xs:documentation>The commission type.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="Modifier" type="typeCommissionModifier" use="optional">                <xs:annotation>                    <xs:documentation>Optional commission modifier.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="Amount" type="typeMoney" use="optional">                <xs:annotation>                    <xs:documentation>The monetary amount of the commission.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="Percentage" type="typePercentageWithDecimal" use="optional">                <xs:annotation>                    <xs:documentation>The percent of the commission.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="BookingTravelerRef" type="typeRef" use="optional">                <xs:annotation>                    <xs:documentation>A reference to a passenger.</xs:documentation>                </xs:annotation>            </xs:attribute>        </xs:complexType>    </xs:element>       <xs:element name="DeliveryInfo">        <xs:annotation>            <xs:documentation>Container to encapsulate all delivery related information</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:sequence>                <xs:element name="ShippingAddress" minOccurs="0">                    <xs:complexType>                        <xs:complexContent>                            <xs:extension base="typeStructuredAddress"/>                        </xs:complexContent>                    </xs:complexType>                </xs:element>                <xs:element ref="PhoneNumber" minOccurs="0"/>                <xs:element ref="Email" minOccurs="0"/>                <xs:element ref="GeneralRemark" minOccurs="0" maxOccurs="unbounded"/>            </xs:sequence>            <xs:attribute name="Type" type="xs:string" use="optional">                <xs:annotation>                    <xs:documentation>An arbitrary identifier to categorize this delivery info</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="SignatureRequired" use="optional">                <xs:annotation>                    <xs:documentation>Indicates whether a signature shoud be required in order to make the delivery.</xs:documentation>                </xs:annotation>                <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:maxLength value="10"/>                    </xs:restriction>                </xs:simpleType>            </xs:attribute>            <xs:attribute name="TrackingNumber" type="xs:string" use="optional">                <xs:annotation>                    <xs:documentation>The tracking number of the shipping company making the delivery.</xs:documentation>                </xs:annotation>            </xs:attribute>        </xs:complexType>    </xs:element>    <xs:element name="ActionStatus">        <xs:annotation>            <xs:documentation>Status of the action that will happen or has happened to the air reservation. One Action status for each provider reservation</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:sequence>                <xs:element ref="Remark" minOccurs="0" />            </xs:sequence>            <xs:attribute name="Type" use="required">                <xs:annotation>                    <xs:documentation>                        Identifies the type of action (if any) to take on this air reservation. Only TTL, TAU, TAX and TAW can be set by the user.                    </xs:documentation>                </xs:annotation>                <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:enumeration value="TAW" />                        <xs:enumeration value="TTL" />                        <xs:enumeration value="TLCXL" />                        <xs:enumeration value="ACTIVE" />                        <xs:enumeration value="CXL" />						<xs:enumeration value="TAU">							<xs:annotation>								<xs:documentation>Equivalent to TAX in Worldspan</xs:documentation>							</xs:annotation>						</xs:enumeration>						<xs:enumeration value="NA">                            <xs:annotation>                                <xs:documentation>                                    NOT APPLICABLE.                                </xs:documentation>                            </xs:annotation>                        </xs:enumeration>                        <xs:enumeration value="TRH" />                    </xs:restriction>                </xs:simpleType>            </xs:attribute>            <xs:attribute name="TicketDate" type="xs:string"                use="optional">                <xs:annotation>                    <xs:documentation>                        Identifies when the action type will happen, or                        has happened according to the type.                    </xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="Key" type="typeRef" use="optional">                <xs:annotation>                    <xs:documentation>                        Identifies when the action type will happen, or                        has happened according to the type.                    </xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="ProviderReservationInfoRef"                type="typeRef" use="optional">                <xs:annotation>                    <xs:documentation>                        Provider reservation reference key.                    </xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="QueueCategory" type="typeNonBlanks" use="optional">                <xs:annotation>                    <xs:documentation>                        Add Category placement to ticketing queue (required in 1P - default is 00)                    </xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="AirportCode" type="typeAirport" use="optional">               	<xs:annotation>                  	<xs:documentation>Used with Time Limit to specify the airport location where the ticket is to be issued.</xs:documentation>               	</xs:annotation>            </xs:attribute>            <xs:attributeGroup ref="attrProviderSupplier">               	<xs:annotation>                 	<xs:documentation>Contains the Provider Code of the provider for which this ActionStatus is used and the airline/vendor code issuing the ticket. Used with Time Limit and TAW.</xs:documentation>   				</xs:annotation>  			</xs:attributeGroup>  			<xs:attribute name="PseudoCityCode" type="typePCC" use="optional">   				<xs:annotation>    				<xs:documentation>The Branch PCC in the host system where PNR can be queued for ticketing. When used with TAU it will auto queue to Q10. When used with TAW agent performs manual move to Q.</xs:documentation>   				</xs:annotation>  			</xs:attribute>  			<xs:attribute name="AccountCode" type="xs:string" use="optional">   				<xs:annotation>    				<xs:documentation>Used with TAW. Used to specify a corporate or in house account code to the PNR as part of ticketing arrangement field.</xs:documentation>   				</xs:annotation>  			</xs:attribute>        </xs:complexType>    </xs:element>    <xs:element name="Endorsement">        <xs:annotation>            <xs:documentation>Restrictions or instructions about the fare or ticket</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="Value" type="typeEndorsement" use="required"/>        </xs:complexType>    </xs:element>    <xs:element name="LoyaltyCard">      <xs:annotation>         <xs:documentation>Provider loyalty card information</xs:documentation>      </xs:annotation>      <xs:complexType>         <xs:sequence>            <xs:element ref="OperatedBy" minOccurs="0" maxOccurs="unbounded"/>         </xs:sequence>         <xs:attribute name="Key" type="typeRef" use="optional"/>                  <xs:attribute name="SupplierCode" type="typeCarrier" use="required"/>         <xs:attribute name="CardNumber" type="typeCardNumber" use="required"/>         <xs:attribute name="Status" type="xs:string" use="optional"/>         <xs:attribute name="MembershipStatus" type="xs:string" use="optional">            <xs:annotation>               <xs:documentation>Loyalty Program membership status of the traveler</xs:documentation>            </xs:annotation>         </xs:attribute>         <xs:attribute name="FreeText" type="xs:string" use="optional"/>         <xs:attribute name="SupplierType" type="typeProduct" use="optional"/>         <xs:attribute name="Level" use="optional">            <xs:simpleType>               <xs:restriction base="xs:integer">                  <xs:minInclusive value="1"/>                  <xs:maxInclusive value="9"/>               </xs:restriction>            </xs:simpleType>         </xs:attribute>         <xs:attribute name="MembershipProgram" type="StringLength1to32" use="optional">            <xs:annotation>               <xs:documentation>Loyalty Program membership Id of the traveler specific to Amtrak(2V) Guest Rewards</xs:documentation>            </xs:annotation>         </xs:attribute>               </xs:complexType>   </xs:element>      <xs:element name="DiscountCard">      <xs:annotation>         <xs:documentation>Rail Discount Card Information</xs:documentation>      </xs:annotation>      <xs:complexType>         <xs:attribute name="Key" type="typeRef" use="optional"/>                  <xs:attribute name="Code" type="StringLength1to8" use="required"/>         <xs:attribute name="Description" type="StringLength1to255" use="optional"/>         <xs:attribute name="Number" type="typeCardNumber" use="optional"/>      </xs:complexType>   </xs:element>      <xs:element name="OperatedBy">      <xs:annotation>         <xs:documentation>This is the carrier code to support Cross Accrual</xs:documentation>      </xs:annotation>      <xs:complexType>         <xs:simpleContent>            <xs:extension base="typeNonBlanks"/>         </xs:simpleContent>      </xs:complexType>   </xs:element>    <xs:element name="Penalty">        <xs:annotation>            <xs:documentation>Exchange penalty information</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="CancelRefund" type="xs:boolean" use="optional"/>            <xs:attribute name="NonRefundable" type="xs:boolean" use="optional"/>            <xs:attribute name="NonExchangeable" type="xs:boolean" use="optional"/>            <xs:attribute name="CancelationPenalty" type="xs:boolean" use="optional"/>            <xs:attribute name="ReissuePenalty" type="xs:boolean" use="optional"/>            <xs:attribute name="NonReissuePenalty" type="xs:boolean" use="optional"/>            <xs:attribute name="TicketRefundPenalty" type="xs:boolean" use="optional"/>            <xs:attribute name="ChargeApplicable" type="xs:boolean" use="optional"/>            <xs:attribute name="ChargePortion" type="xs:boolean" use="optional"/>            <xs:attribute name="PenaltyAmount" type="typeMoney" use="optional"/>        </xs:complexType>    </xs:element>    <xs:element name="Restriction">        <xs:annotation>            <xs:documentation>Which activities are supported for a particular element</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="Operation" type="xs:string" use="required">                <xs:annotation>                    <xs:documentation>The operation that is restricted</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="Reason" type="xs:string" use="optional">                <xs:annotation>                    <xs:documentation>The reason it is restricted</xs:documentation>                </xs:annotation>            </xs:attribute>        </xs:complexType>    </xs:element>        <xs:element name="AirSeatAssignment">        <xs:annotation>            <xs:documentation>Identifies the seat assignment for a passenger.</xs:documentation>        </xs:annotation>        <xs:complexType>    		<xs:attribute name="Key" type="typeRef" use="optional"/>            <xs:attribute name="Status" type="typeStatusCode" use="required"/>            <xs:attribute name="Seat" type="xs:string" use="required"/>            <xs:attribute name="SeatTypeCode" type="typeSeatTypeCode" use="optional">				<xs:annotation>					<xs:documentation>The 4 letter SSR code like SMSW,NSSW,SMST etc.</xs:documentation>				</xs:annotation>			</xs:attribute>            <xs:attribute name="SegmentRef" type="typeRef" use="optional"/>            <xs:attribute name="FlightDetailsRef" type="typeRef" use="optional"/>			        </xs:complexType>    </xs:element>    <xs:element name="RailSeatAssignment">        <xs:annotation>            <xs:documentation>Identifies the seat assignment for a passenger on RailSegment.</xs:documentation>        </xs:annotation>        <xs:complexType>        	<xs:sequence>              <xs:element ref="Characteristic" minOccurs="0" maxOccurs="unbounded"/>            </xs:sequence>    		<xs:attribute name="Key" type="typeRef" use="optional"/>            <xs:attribute name="Status" type="typeStatusCode" use="required"/>            <xs:attribute name="Seat" type="xs:string" use="required"/>			<xs:attribute name="RailSegmentRef" type="typeRef" use="optional"/>            <xs:attribute name="CoachNumber" type="xs:string" use="optional"/>                    </xs:complexType>    </xs:element>	<xs:element name="Characteristic">		<xs:annotation>			<xs:documentation>Identifies the characteristics of the seat with seat type, value and description.</xs:documentation>		</xs:annotation>        <xs:complexType>         <xs:attribute name="SeatType" use="optional">                <xs:annotation>                    <xs:documentation>Indicates codeset of values such as Seat Type like Place,Position, Smoking Choice, Place Arrangement, Place Direction, Compartment. </xs:documentation>                </xs:annotation>                <xs:simpleType>                   <xs:restriction base="xs:string">                      <xs:minLength value="0"/>                      <xs:maxLength value="255"/>                   </xs:restriction>                </xs:simpleType>            </xs:attribute>	         <xs:attribute name="SeatDescription" use="optional">	                <xs:annotation>	                    <xs:documentation>Description of the seat type.</xs:documentation>	                </xs:annotation>	                <xs:simpleType>	                   <xs:restriction base="xs:string">	                      <xs:minLength value="0"/>	                      <xs:maxLength value="255"/>	                   </xs:restriction>	                </xs:simpleType>	            </xs:attribute>                                                      	            <xs:attribute name="SeatValue" use="optional">	                <xs:annotation>	                    <xs:documentation>Indicates the value specific to the selected type.</xs:documentation>	                </xs:annotation>	                <xs:simpleType>	                   <xs:restriction base="xs:string">	                      <xs:minLength value="0"/>	                      <xs:maxLength value="255"/>	                   </xs:restriction>	                </xs:simpleType>                	            </xs:attribute>	         	<xs:attribute name="SeatValueDescription" use="optional">	                <xs:annotation>	                    <xs:documentation>Description of the seat value.</xs:documentation>	                </xs:annotation>	                <xs:simpleType>	                   <xs:restriction base="xs:string">	                      <xs:minLength value="0"/>	                      <xs:maxLength value="255"/>	                   </xs:restriction>	                </xs:simpleType>	            </xs:attribute>                       </xs:complexType>    </xs:element>  	    <xs:simpleType name="typeSeatTypeCode">        <xs:annotation>            <xs:documentation>Valid 4 letter Seat Type Code</xs:documentation>        </xs:annotation>        <xs:restriction base="xs:string">            <xs:length value="4"/>            <xs:whiteSpace value="collapse"/>        </xs:restriction>    </xs:simpleType>    <xs:simpleType name="typeStatusCode">        <xs:annotation>            <xs:documentation>Valid 2 letter Status Code</xs:documentation>        </xs:annotation>        <xs:restriction base="xs:string">            <xs:length value="2"/>            <xs:whiteSpace value="collapse"/>        </xs:restriction>    </xs:simpleType>        <xs:element name="ServiceData">        <xs:complexType>            <xs:attribute name="Data" type="xs:string" use="optional">                <xs:annotation>                    <xs:documentation>Data that specifies the details of the merchandising offering (e.g. seat number for seat service)</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="AirSegmentRef" type="typeRef" use="optional">                <xs:annotation>                    <xs:documentation>Reference to a segment if the merchandising offering only pertains to that segment. If no segment reference is present this means this offering is for the whole itinerary.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="BookingTravelerRef" type="typeRef" use="optional">                <xs:annotation>                    <xs:documentation>Reference to a passenger if the merchandising offering only pertains to that passenger. If no passenger reference is present this means this offering is for all passengers.</xs:documentation>                </xs:annotation>            </xs:attribute>         </xs:complexType>    </xs:element>    <xs:element name="ServiceInfo">        <xs:complexType>           <xs:sequence>              <xs:element name="Description" type="xs:string">                 <xs:annotation>                    <xs:documentation>Description of the Service.  Usually used in tandem with  one or more media items.</xs:documentation>                 </xs:annotation>              </xs:element>              <xs:element ref="MediaItem" minOccurs="0" maxOccurs="2"/>           </xs:sequence>        </xs:complexType>    </xs:element>    <xs:element name="SSR">		<xs:annotation>			<xs:documentation>Special serivces like wheel chair, or pet carrier.</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="Key" type="typeRef" use="optional"/>			<xs:attribute name="SegmentRef" type="typeRef" use="optional"/>			<xs:attribute name="PassiveSegmentRef" type="typeRef" use="optional">				<xs:annotation>					<xs:documentation>Reference to the passive segment.</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="Type" use="required" type="typeSSRCode" />			<xs:attribute name="Status" type="xs:string" use="optional"/>			<xs:attribute name="FreeText" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Certain SSR types will require a free text message. For example MAAS (Meet and assist).</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="Carrier" type="typeCarrier" use="optional"/>			<xs:attribute name="CarrierSpecificText" use="optional">				<xs:annotation>					<xs:documentation>Carrier specific information which are not captured in the FreeText field(not present in IATA's standard SSR DOCO format). An example is VISA Expiration Date.</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="1"/>						<xs:maxLength value="64"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="Description" type="xs:string" use="optional"/>			<xs:attribute name="ProviderDefinedType" use="optional">				<xs:annotation>					<xs:documentation>Original Type as sent by the provider</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="1"/>						<xs:maxLength value="16"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="SSRRuleRef" type="typeRef" use="optional">				<xs:annotation>					<xs:documentation>UniqueID to associate a rule to the SSR</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="URL" type="xs:anyURI"/>		</xs:complexType>	</xs:element>    <xs:element name="TravelerType">        <xs:annotation>            <xs:documentation>The 3-char IATA traveler type code</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="Code" type="typePTC" use="required"/>        </xs:complexType>    </xs:element>    <xs:element name="TicketNumber">        <xs:annotation>            <xs:documentation>The identifying number for the actual ticket</xs:documentation>        </xs:annotation>        <xs:simpleType>            <xs:restriction base="xs:string">                <xs:length value="13"/>            </xs:restriction>        </xs:simpleType>    </xs:element>     <xs:element name="BookingTravelerRef">        <xs:annotation>            <xs:documentation>Reference Element for Booking Traveler and Loyalty cards</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:sequence>                <xs:element ref="LoyaltyCardRef" minOccurs="0" maxOccurs="unbounded"/>                <xs:element ref="DriversLicenseRef" minOccurs="0"/>            </xs:sequence>            <xs:attribute name="Key" type="typeRef" use="optional"/>            <xs:attribute name="Primary" type="xs:boolean" use="optional"/>        </xs:complexType>    </xs:element>        <xs:element name="DriversLicenseRef">        <xs:complexType>            <xs:annotation>                <xs:documentation>Reference Element for Drivers Licenses</xs:documentation>            </xs:annotation>            <xs:attribute name="Key" type="typeRef" use="required"/>        </xs:complexType>    </xs:element>    <xs:element name="LoyaltyCardRef">        <xs:complexType>            <xs:annotation>                <xs:documentation>Reference Element for Loyalty cards</xs:documentation>            </xs:annotation>            <xs:attribute name="Key" type="typeRef" use="required"/>        </xs:complexType>    </xs:element>        <xs:complexType name="typeRemark">        <xs:simpleContent>           <xs:extension base="xs:string">              <xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">                 <xs:annotation>                    <xs:documentation>Provider reservation reference key.</xs:documentation>                 </xs:annotation>              </xs:attribute>              <xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">                 <xs:annotation>                    <xs:documentation>Contains the Provider Code of the provider for which this element is used</xs:documentation>                 </xs:annotation>              </xs:attribute>           </xs:extension>        </xs:simpleContent>    </xs:complexType>        <xs:element name="UnassociatedRemark">	  <xs:annotation>	     <xs:documentation>A textual remark container to hold non-associated itinerary remarks</xs:documentation>	  </xs:annotation>	  <xs:complexType>	  	<xs:complexContent>	    	<xs:extension base="typeRemarkWithTravelerRef">	    		<xs:attribute name="Key" type="typeRef" use="optional"/>	        </xs:extension>	  	</xs:complexContent>	  </xs:complexType>	</xs:element>		<xs:complexType name="typeRemarkWithTravelerRef">        <xs:sequence>        <xs:element name="RemarkData" type="xs:string">            <xs:annotation>                <xs:documentation>Actual remarks data.</xs:documentation>            </xs:annotation>        </xs:element>        <xs:element name="BookingTravelerRef" type="typeRef" minOccurs="0" maxOccurs="unbounded">            <xs:annotation>                <xs:documentation>Reference to Booking Traveler.</xs:documentation>            </xs:annotation>        </xs:element>        </xs:sequence>        <xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">            <xs:annotation>                <xs:documentation>Provider reservation reference key.</xs:documentation>            </xs:annotation>        </xs:attribute>        <xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">            <xs:annotation>                <xs:documentation>Contains the Provider Code of the provider for which this element is used</xs:documentation>            </xs:annotation>        </xs:attribute>    </xs:complexType>	<xs:element name="CustomerID">		<xs:annotation>	 	   <xs:documentation>A provider reservation field used to store customer information. It may be used to identify reservations which will/will not be available for access.</xs:documentation>	    </xs:annotation>		<xs:complexType>	 	   <xs:complexContent>		 	  <xs:extension base="typeRemark">		 	     <xs:attribute name="Key" type="typeRef" use="optional"/>		      </xs:extension>		   </xs:complexContent>		</xs:complexType>	</xs:element>	    <!--*** PNR Reference Elements ***-->    <xs:complexType name="BaseReservation">        <xs:sequence>            <!-- <xs:element ref="BookingTraveler" minOccurs="0" maxOccurs="unbounded"/>-->            <xs:element ref="AccountingRemark" minOccurs="0" maxOccurs="unbounded"/>            <xs:element ref="GeneralRemark" minOccurs="0" maxOccurs="unbounded"/>            <xs:element ref="Restriction" minOccurs="0" maxOccurs="unbounded"/>            <xs:element ref="PassiveInfo" minOccurs="0"/>                    </xs:sequence>        <xs:attribute name="LocatorCode" type="typeLocatorCode" use="required">            <xs:annotation>                <xs:documentation>The unique identifier for this reservation</xs:documentation>            </xs:annotation>        </xs:attribute>        <xs:attribute name="CreateDate" type="xs:string" use="required">            <xs:annotation>                <xs:documentation>The date and time that this reservation was created.</xs:documentation>            </xs:annotation>        </xs:attribute>        <xs:attribute name="ModifiedDate" type="xs:string" use="required">            <xs:annotation>                <xs:documentation>The date and time that this reservation was last modified for any reason.</xs:documentation>            </xs:annotation>        </xs:attribute>        <xs:attribute name="CustomerNumber" type="xs:string" use="optional"/>    </xs:complexType>    <xs:element name="MCO">        <xs:complexType>            <xs:sequence>                <xs:element ref="AgencyInfo"/>                <xs:element ref="Name"/>                <xs:element ref="FormOfPayment" minOccurs="0"/>                <xs:element ref="GeneralRemark" minOccurs="0" maxOccurs="unbounded"/>                <xs:element ref="Endorsement" minOccurs="0" maxOccurs="unbounded"/>                <xs:element ref="Restriction" minOccurs="0" maxOccurs="unbounded"/>            </xs:sequence>            <xs:attribute name="Number" type="xs:string" use="required">                <xs:annotation>                    <xs:documentation>The unique MCO number</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="Type" type="typeMCOType" use="required">                <xs:annotation>                    <xs:documentation>The Type of MCO. Once of Agency Fee, Airline Service Fee, or Residual value from an Exchange.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="ServiceFeeType" type="typeMCOFeeType" use="optional">                <xs:annotation>                    <xs:documentation>Only relavant for an Airline Service Fee. Identifies the reason for the fee.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="Status" type="typeMCOStatus" use="required">                <xs:annotation>                    <xs:documentation>Current status of the MCO</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="CreateDate" type="xs:dateTime" use="required">                <xs:annotation>                    <xs:documentation>The date the MCO was issued</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="ModifyDate" type="xs:dateTime" use="optional">                <xs:annotation>                    <xs:documentation>The date the MCO was last changed either due to agent interaction, or a system update</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="CarrierCode" type="typeCarrier" use="required">                <xs:annotation>                    <xs:documentation>The carrier code or provider code that will be accepting the MCO</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="LocationCode" type="typeAirport" use="optional">                <xs:annotation>                    <xs:documentation>The airport code where the MCO will be accepted, or the City code where the requesting agency resides.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="LocatorCode" type="xs:string" use="optional">                <xs:annotation>                    <xs:documentation>The locator code of the PNR that the MCO was created for.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="TicketNumber" type="xs:string" use="optional">                <xs:annotation>                    <xs:documentation>The ticket that this MCO was issued in connection with. Could be the ticket that caused the fee, a residual from an exchange, or an airline service fee.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="Amount" type="typeMoney" use="required">                <xs:annotation>                    <xs:documentation>The total value of the MCO including any processing fees.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="NetAmount" type="typeMoney" use="optional">                <xs:annotation>                    <xs:documentation>The net amount of value minus the processing fee.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="ProcessingFee" type="typeMoney" use="optional">                <xs:annotation>                    <xs:documentation>The amount charged by the provider to process the MCO. ARC is the typical provider.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="Refundable" type="xs:boolean" use="required">                <xs:annotation>                    <xs:documentation>Indicates if this MCO is refundable or not</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="Voidable" type="xs:boolean" use="required">                <xs:annotation>                    <xs:documentation>Indicates if this MCO is voidable or not</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="Exchangeable" type="xs:boolean" use="required">                <xs:annotation>                    <xs:documentation>Indicates that this MCO was created from the residual value of an exchange and can be used as a form of payment in a new booking.</xs:documentation>                </xs:annotation>            </xs:attribute>        </xs:complexType>    </xs:element>    <xs:element name="MetaData">        <xs:annotation>            <xs:documentation>Extra data to elaborate the parent element. This data is primarily informative and is not persisted.</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="Key" use="required">                <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:maxLength value="10"/>                        <xs:minLength value="1"/>                    </xs:restriction>                </xs:simpleType>            </xs:attribute>            <xs:attribute name="Value" use="required">                <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:maxLength value="50"/>                        <xs:minLength value="1"/>                    </xs:restriction>                </xs:simpleType>            </xs:attribute>        </xs:complexType>    </xs:element>    <xs:element name="RoleInfo">        <xs:annotation>            <xs:documentation>Container to specify the role of the agent</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="Id" use="required">                <xs:annotation>                     <xs:documentation>Unique identifier of the role.</xs:documentation>                </xs:annotation>                <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:maxLength value="19"/>                    </xs:restriction>                </xs:simpleType>            </xs:attribute>            <xs:attribute name="Name" use="required">                <xs:annotation>                     <xs:documentation>Agent's role name </xs:documentation>                </xs:annotation>                <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:maxLength value="128"/>                    </xs:restriction>                </xs:simpleType>            </xs:attribute>            <xs:attribute name="Source"  type="xs:string" use="optional">            <xs:annotation>                 <xs:documentation>Role inheritance level. Needed in the response, not in the request </xs:documentation>            </xs:annotation>            </xs:attribute>            <xs:attribute name="Description" use="optional">                <xs:annotation>                    <xs:documentation>Description of role</xs:documentation>                </xs:annotation>                <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:maxLength value="1024"/>                    </xs:restriction>                </xs:simpleType>            </xs:attribute>         </xs:complexType>     </xs:element>    <xs:element name="OSI">        <xs:annotation>            <xs:documentation>Other Service information sent to the carriers during air bookings</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="Key" type="typeRef" use="optional"/>            <xs:attribute name="Carrier" type="typeCarrier" use="required"/>            <xs:attribute name="Code" use="optional">                <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:maxLength value="4"/>                    </xs:restriction>                </xs:simpleType>            </xs:attribute>            <xs:attribute name="Text" use="required">                <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:maxLength value="256"/>                    </xs:restriction>                </xs:simpleType>            </xs:attribute>            <xs:attribute name="ProviderReservationInfoRef" type="typeRef" use="optional">                <xs:annotation>                    <xs:documentation>Provider reservation reference key.</xs:documentation>                </xs:annotation>            </xs:attribute>            <xs:attribute name="ProviderCode" type="typeProviderCode" use="optional">                <xs:annotation>                      <xs:documentation>Contains the Provider Code of the provider for which this OSI is used</xs:documentation>                </xs:annotation>            </xs:attribute>        </xs:complexType>    </xs:element>    <xs:complexType name="Segment">        <xs:annotation>            <xs:documentation>The base segment type</xs:documentation>        </xs:annotation>        <xs:sequence>            <xs:element ref="SegmentRemark" minOccurs="0" maxOccurs="unbounded"/>        </xs:sequence>        <xs:attribute name="Key" type="typeRef" use="required"/>        <xs:attribute name="Status" type="xs:string" use="optional">            <xs:annotation>                <xs:documentation>Status of this segment.</xs:documentation>            </xs:annotation>        </xs:attribute>        <xs:attribute name="Passive" type="xs:boolean" use="optional"/>        <xs:attribute name="TravelOrder" type="xs:integer" use="optional">            <xs:annotation>                <xs:documentation>To identify the appropriate travel sequence for Air/Car/Hotel segments/reservations based on travel dates. This ordering is applicable across the UR not provider or traveler specific</xs:documentation>            </xs:annotation>        </xs:attribute>    </xs:complexType>    <xs:element name="TravelSegment">        <xs:annotation>            <xs:documentation>Generic segment used to provide travel information that was not processed by the system</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:complexContent>                <xs:extension base="Segment">                    <xs:attribute name="Origin" type="typeIATACode" use="optional">                        <xs:annotation>                            <xs:documentation>The IATA location code for this origination of this entity.</xs:documentation>                        </xs:annotation>                    </xs:attribute>                    <xs:attribute name="Destination" type="typeIATACode" use="optional">                        <xs:annotation>                            <xs:documentation>The IATA location code for this destination of this entity.</xs:documentation>                        </xs:annotation>                    </xs:attribute>                    <xs:attribute name="DepartureTime" type="xs:string" use="optional">                        <xs:annotation>                            <xs:documentation>The date and time at which this entity departs. This does not include time zone information since it can be derived from the origin location.</xs:documentation>                        </xs:annotation>                    </xs:attribute>                    <xs:attribute name="ArrivalTime" type="xs:string" use="optional">                        <xs:annotation>                            <xs:documentation>The date and time at which this entity arrives at the destination. This does not include time zone information since it can be derived from the origin location.</xs:documentation>                        </xs:annotation>                    </xs:attribute>                </xs:extension>            </xs:complexContent>        </xs:complexType>    </xs:element>    <!--*** Search Data Types ***-->    <xs:element name="SearchEvent">        <xs:annotation>            <xs:documentation>Search for various reservation events</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:complexContent>                <xs:extension base="typeTimeRange">                    <xs:attribute name="Type" type="typeEventType"/>                </xs:extension>            </xs:complexContent>        </xs:complexType>    </xs:element>    <xs:element name="SearchTicketing">        <xs:annotation>            <xs:documentation>Search restriction by Agent</xs:documentation>        </xs:annotation>        <xs:complexType>            <xs:attribute name="TicketStatus" use="optional" default="Both">                <xs:annotation>                    <xs:documentation>Return only PNRs with ticketed, non-ticketed or both</xs:documentation>                </xs:annotation>                <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:enumeration value="Ticketed"/>                        <xs:enumeration value="Unticketed"/>                        <xs:enumeration value="Both"/>                    </xs:restriction>                </xs:simpleType>            </xs:attribute>            <xs:attribute name="ReservationStatus" use="optional" default="Both">                <xs:annotation>                    <xs:documentation>Used only if "TicketStatus" set to "No" or "Both". Return only PNRs with specific reservation status or both statuses.</xs:documentation>                </xs:annotation>                <xs:simpleType>                    <xs:restriction base="xs:string">                        <xs:enumeration value="OnHold"/>                        <xs:enumeration value="SetForTicketing"/>                        <xs:enumeration value="Both"/>                    </xs:restriction>                </xs:simpleType>            </xs:attribute>            <xs:attribute name="TicketDate" type="xs:date" use="optional">                <xs:annotation>                    <xs:documentation>Identifies when this reservation was ticketed, or when it should be ticketed by (in the event of a TTL)</xs:documentation>                </xs:annotation>            </xs:attribute>        </xs:complexType>    </xs:element>    <xs:element name="SearchPassenger">        <xs:annotation>            <xs:documentation>Passenger type with code and optional age information</xs:documentation>        </xs:annotation>        <xs:complexType>	        <xs:complexContent>	         	<xs:extension base="typePassengerType">                    <xs:attribute name="Key" type="typeRef" use="optional"/>                </xs:extension>     	        </xs:complexContent>              </xs:complexType>    </xs:element>    <xs:element name="SimpleName" type="xs:string">        <xs:annotation>            <xs:documentation>Free text name</xs:documentation>        </xs:annotation>    </xs:element>    <xs:element name="SpecialEquipment">        <xs:complexType>            <xs:attribute name="Key" type="typeRef" use="optional"/>            <xs:attribute