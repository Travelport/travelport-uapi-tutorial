          <xs:documentation>A tag that belongs to the agency.</xs:documentation>
         </xs:annotation>
        </xs:element>
       </xs:sequence>
      </xs:extension>
     </xs:complexContent>
    </xs:complexType>
   </xs:element>   

   <xs:element name="ProfileDeleteTagReq">
