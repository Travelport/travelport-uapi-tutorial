    <!--*** Shared reference elements ***-->
    <xs:element name="VehicleSearchModifiers">
      <xs:annotation>
         <xs:documentation>Controls and switches for the Vehicle Search request</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element name="PermittedVendors" minOccurs="0">
               <xs:complexType>
                  <xs:sequence>
                     <xs:element ref="Vendor" maxOccurs="unbounded">
                         <xs:annotation>
                             <xs:documentation>1G/1V max 4 vendors, 1P/1J max 99 vendors</xs:documentation>
                         </xs:annotation>
                     </xs:element>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
            <xs:element name="ProhibitedVendors" minOccurs="0">
               <xs:complexType>
                  <xs:sequence>
                     <xs:element ref="Vendor" maxOccurs="unbounded"/>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
            <xs:element ref="VehicleModifier" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="VehicleType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="RateModifiers" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="RateHostIndicator" type="typeRateHostIndicator" minOccurs="0"/>
            <xs:element ref="common:LoyaltyCard" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="common:ReferencePoint" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Search Car by reference point</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element ref="common:BookingSource" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Supported Providers: 1P/1J. Only type IataNumber is valid.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element ref="common:SpecialEquipment" minOccurs="0" maxOccurs="5">
               <xs:annotation>
                  <xs:documentation>Allows vehicle search with Special equipment by specifying special equipment type. e.g. “BikeRack”. Not supported by all vendors. 1P/1J only.</xs:documentation>
               </xs:annotation>
            </xs:element>
			<xs:element name="SearchDistance" type="typeVehicleSearchDistance" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Distance from search location. Supported for standard car search.  Providers: 1g/1v</xs:documentation>
               </xs:annotation>
            </xs:element>     
            <xs:element ref="Policy" minOccurs="0" maxOccurs="999"/>
         </xs:sequence>
         <xs:attribute name="Key" type="common:typeRef" use="optional"/>
         <xs:attribute name="PreferredCurrency" type="common:typeCurrency" use="optional">
            <xs:annotation>
               <xs:documentation>Alternate currency</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="UnlimitedMileage" type="xs:boolean" use="optional">
            <xs:annotation>
               <xs:documentation>Set to true to search for rates with unlimited mileage.Defaults to false.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RateCategory" type="common:typeRateCategory" use="optional">
                <xs:annotation>
                    <xs:documentation>The category of this rate (Best, etc)</xs:documentation>
                </xs:annotation>
        </xs:attribute>
        <xs:attribute name="RateGuaranteed" type="xs:boolean" use="optional" default="false">
               <xs:annotation>
                    <xs:documentation>Guarantee indicator for vehicle rate.</xs:documentation>
               </xs:annotation>
         </xs:attribute>
         <xs:attribute name="RatePeriod" type="common:typeRateTimePeriod" use="optional">
                <xs:annotation>
                    <xs:documentation>The period for the rate code (daily, weekly, etc)</xs:documentation>
                </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SellableRatesOnly" type="xs:boolean" use="optional">
            <xs:annotation>
               <xs:documentation>Set to true to search for sellable rates only.  Default is to search for all rates.  Providers: 1p/1j</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
  <xs:complexType name="typeVehicleSearchDistance">
      <xs:attribute name="Units" type="common:typeDistance" use="optional">
         <xs:annotation>
            <xs:documentation>MI or KM. Defaults to miles</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Direction" use="optional">
         <xs:annotation>
            <xs:documentation>Valid values: N, S, E, W, NE, NW, SE, SW.  Default:  direction will not be specified.</xs:documentation>
         </xs:annotation>
         <xs:simpleType>
            <xs:restriction base="xs:string">
               <xs:maxLength value="2"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="MinDistance" use="optional">
         <xs:annotation>
            <xs:documentation>Minimum distance.  Defaults to 0.</xs:documentation>
         </xs:annotation>
         <xs:simpleType>
            <xs:restriction base="xs:nonNegativeInteger">
               <xs:minInclusive value="0"/>
               <xs:maxInclusive value="999"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="MaxDistance" use="required">
         <xs:annotation>
            <xs:documentation>Maximum distance</xs:documentation>
         </xs:annotation>
         <xs:simpleType>
            <xs:restriction base="xs:nonNegativeInteger">
               <xs:maxInclusive value="999"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:complexType>
  
   
