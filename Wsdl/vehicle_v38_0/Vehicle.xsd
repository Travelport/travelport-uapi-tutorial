<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.travelport.com/schema/vehicle_v38_0" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.travelport.com/schema/vehicle_v38_0"
           xmlns:common="http://www.travelport.com/schema/common_v38_0"           
           elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:annotation>
        <xs:documentation>(c) Travelport GDS 2008 All rights reserved. Proprietary Information of Travelport GDS.</xs:documentation>
    </xs:annotation>

    <xs:import schemaLocation="../common_v38_0/CommonReqRsp.xsd" namespace="http://www.travelport.com/schema/common_v38_0"/>

    <xs:element name="VehicleReservation">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReservation">
                    <xs:sequence>
                        <xs:element ref="common:BookingTravelerRef" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:group ref="BaseVehicleReservationGroup"/>
                    </xs:sequence>
                    <xs:attribute name="SupplierCode" type="xs:string" use="required"/>
                    <xs:attribute name="BookingConfirmation" type="xs:string" use="optional"/>
                    <xs:attribute name="Status" type="xs:string" use="required"/>
                    <xs:attribute name="ProviderReservationInfoRef" type="common:typeRef" use="optional">
                        <xs:annotation>
                            <xs:documentation>Provider reservation reference key.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="PassiveProviderReservationInfoRef" type="common:typeRef" use="optional">
                        <xs:annotation>
                            <xs:documentation>Passive Provider reservation reference key.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="TravelOrder" type="xs:integer" use="optional">
                        <xs:annotation>
                            <xs:documentation>To identify the appropriate sequence for Air/Car/Hotel segments based on travel dates.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:group name="BaseVehicleReservationGroup">
        <xs:sequence>
            <xs:element ref="common:ReservationName" minOccurs="0"/>
            <xs:element ref="VehicleDateLocation"/>
            <xs:element ref="Vehicle"/>
            <xs:element ref="common:SpecialEquipment" minOccurs="0" maxOccurs="5"/>
            <xs:element ref="VehicleSpecialRequest" minOccurs="0"/>
            <xs:element ref="PaymentInformation" minOccurs="0"/>
            <xs:element ref="DeliveryAddress" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>An address to which a rental car should be delivered and a phone number associated with the address.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="CollectionAddress" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>An address from which a rental car should be picked up at the end of a rental period and a phone number associated with the address.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="FlightArrivalInformation" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The flight arrival information (airline code and flight number) for the airport/city at which the rental car will be picked up</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="common:Guarantee" minOccurs="0"/>
            <xs:element ref="AssociatedRemark" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="common:BookingSource" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Specify alternate booking source</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="common:ThirdPartyInformation" minOccurs="0"/>
            <xs:element ref="common:SellMessage" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:group>

    <xs:element name="PaymentInformation">
      <xs:annotation>
         <xs:documentation>The payment information for a vehicle reservation.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
             <xs:extension base="typePaymentInformation">
                  <xs:attribute name="Key" type="common:typeRef" use="optional"/>
              </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   
    <xs:element name="VehicleDateLocation">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="VendorLocation" minOccurs="0" maxOccurs="unbounded">
	               <xs:annotation>
	                  <xs:documentation>Specific vendor rental locations.  Applicable only for VehicleSearchAvailability.</xs:documentation>
	               </xs:annotation>
	               <xs:complexType>
	                  <xs:complexContent>
	                     <xs:extension base="typeVehicleVendorLocation">
	                        <xs:sequence>
	                           <xs:element ref="common:Distance" minOccurs="0"/>
	                        </xs:sequence>
	                     </xs:extension>
	                  </xs:complexContent>
	               </xs:complexType>
	            </xs:element>
            </xs:sequence>
            <xs:attribute name="PickupDateTime" type="xs:string" use="required"/>
            <xs:attribute name="PickupLocation" type="common:typeIATACode" use="optional">
                <xs:annotation>
                    <xs:documentation>PickUpLocation is optional if Reference point is specified</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ReturnDateTime" type="xs:string" use="required"/>
            <xs:attribute name="ReturnLocation" type="common:typeIATACode" use="optional">
                <xs:annotation>
                    <xs:documentation>If not specified, the PickupLocation will be assumed</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="PickupLocationType" type="common:typeVehicleLocation" use="optional">
                <xs:annotation>
                    <xs:documentation>Required if use VendorLocationID. Ex: Terminal, ShuttleOnAirport, ShuttleOffAirport, RailwayStation, Hotel, CarDealer, CityCenterDowntown, EastOfCityCenter, SouthOfCityCenter, WestOfCityCenter, NorthOfCityCenter, PortOrFerry, NearResort, Airport, Unknown</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ReturnLocationType" type="common:typeVehicleLocation" use="optional">
                <xs:annotation>
                    <xs:documentation>Defaults to Pickup Location. Same options as Pickup Location.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="PickupLocationNumber" type="xs:string" use="optional">
				<xs:annotation>
                    <xs:documentation>The value of this attribute should be the same as the value of VendorLocationID returned as part of VendorLocation@VendorLocationID in a VehicleLocationRsp.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ReturnLocationNumber" type="xs:string" use="optional"/>
            <xs:attribute name="Key" type="common:typeRef" use="optional"/>
        </xs:complexType>
    </xs:element>
	
	<xs:complexType name="typeVehicleVendorLocation">
	  <xs:complexContent>
		  <xs:extension base="common:typeVendorLocation">
		   	  <xs:attributeGroup ref="attrVehicleVendorLocation"/>
	   	  </xs:extension>	   
	  </xs:complexContent>
	</xs:complexType>
	
	<xs:attributeGroup name="attrVehicleVendorLocation">
		<xs:annotation>
			<xs:documentation>Basic vehicle pickup and drop off location details</xs:documentation>
		</xs:annotation>
		<xs:attribute name="LocationType" type="common:typeVehicleLocation"
			use="optional">
			<xs:annotation>
				<xs:documentation>Rental counter location such as Terminal, CityCenterDowntown,</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LocationCode" type="common:typeIATACode"
			use="optional">
			<xs:annotation>
				<xs:documentation>Airport or City Code</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Type" use="optional">
			<xs:annotation>
				<xs:documentation>Pickup or Return</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Pickup" />
					<xs:enumeration value="Return" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
	
    <xs:element name="Vehicle">
        <xs:annotation>
            <xs:documentation>Information related to single vehicle</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
            	<xs:element name="PolicyCodesList" type="common:typePolicyCodesList" minOccurs="0">
                  <xs:annotation>
                     <xs:documentation>A list of codes that indicate why an item was determined to be ‘out of policy’.</xs:documentation>
                  </xs:annotation>
               	</xs:element>
                <xs:element ref="VehicleRate" minOccurs="0"/>
                <!-- S1352-RateModifier Removed from Vehicle
                <xs:element ref="RateModifiers" minOccurs="0"/> -->
            </xs:sequence>
            <xs:attribute name="VendorCode" type="common:typeSupplierCode" use="required"/>
            <xs:attribute name="AirConditioning" type="xs:boolean" use="required">
                <xs:annotation>
                    <xs:documentation>True or False.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="TransmissionType" type="common:typeVehicleTransmission" use="required">
            	<xs:annotation>
                    <xs:documentation>Automatic, Manual</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="VehicleClass" type="common:typeVehicleClass" use="required">
                <xs:annotation>
                    <xs:documentation>Class of vehicle</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Category" type="common:typeVehicleCategory" use="required">
                <xs:annotation>
                    <xs:documentation>Category of vehicle. Each supplier decides how these categories map to a vehicle class.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Description" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>Car type Description such as 'CHRYSLER SEBRING OR SIMILAR'</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DoorCount" type="common:typeDoorCount" use="optional">
                <xs:annotation>
                    <xs:documentation>The number of doors on vehicle.  Ex: TwoToThreeDoors, TwoToFourDoors, FourToFiveDoors</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Location" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>Location of the Vehicle or Counter</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="CounterLocationCode" use="optional">
	            <xs:annotation>
	               <xs:documentation>Four character Code to identify the Location of the Rental Counter, e.g. OMSO, PHON.</xs:documentation>
	            </xs:annotation>
	            <xs:simpleType>
	               <xs:restriction base="xs:string">
	                  <xs:minLength value="4"/>
	                  <xs:maxLength value="4"/>
	               </xs:restriction>
	            </xs:simpleType>
         	</xs:attribute>
            <xs:attribute name="VendorLocationKey" type="common:typeRef" use="optional">
                <xs:annotation>
                    <xs:documentation>Identifies the specific vendor location</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="VendorName" use="optional">
                <xs:annotation>
                    <xs:documentation>The vendor's name</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="20"/>
                        <xs:minLength value="1"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="AlternateVendor" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>A vendor renting the vehicle on behalf of another company</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="FuelType" type="common:typeFuel" use="optional">
                <xs:annotation>
                   <xs:documentation>The fuel type of vehicle</xs:documentation>
                </xs:annotation>
            </xs:attribute>   
            <xs:attribute name="AcrissVehicleCode" use="optional">
                <xs:annotation>
                   <xs:documentation>The Association of Car Rental Industry System Standards (ACRISS), develops standards to avoid misleading information when making a car rental booking online or via any electronic means. ACRISS provides an industry standard vehicle matrix to define car models ensuring a like to like comparison of vehicle. Each ACRISS code defining a car model consists of four characters as defined. 1st character denotes the vehicle category – based on size, cost, power and luxury factor.2nd character defines the vehicle type – chassis type (van, SUV, wagon, convertible).3rd character defines the transmission and drive – automatic / manual and 2WD / 4WD / AWD.
4th character defines the fuel type (petrol / diesel / hybrid…) and whether air conditioned.Examples are ICAR,ECAR,etc.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="4"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>          
            <xs:attribute name="Key" type="common:typeRef" use="optional"/>
            <xs:attribute name="ReturnAtPickup" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates whether vehicle can be returned at any location or pickup point only. If ReturnAtPickup = false then vehicle can be returned at any location else if ReturnAtPickup = true, vehicle should be returned in Pickup point only. Supported Providers : 1G/1V</xs:documentation>
				</xs:annotation>
			</xs:attribute>
            <xs:attributeGroup ref="common:attrPolicyMarking">
            <xs:annotation>
               <xs:documentation>Attributes related to policy marking.</xs:documentation>
            </xs:annotation>
         	</xs:attributeGroup>
        </xs:complexType>
    </xs:element>
    <xs:element name="VehicleRate">
      <xs:annotation>
         <xs:documentation>Rate summary</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="SupplierRate" minOccurs="0"/>
            <xs:element name="ApproximateRate" type="typeVehicleRates" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Monetary amounts expressed in another currency</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="VehicleRateDescription" type="typeVehicleRateDescription" minOccurs="0" maxOccurs="99"/>
            <xs:element name="RateHostIndicator" type="typeRateHostIndicator" minOccurs="0"/>
            <xs:element name="HourlyLateCharge" type="typeRateInfo" minOccurs="0"/>
            <xs:element name="DailyLateCharge" type="typeRateInfo" minOccurs="0"/>
            <xs:element ref="RateInclusions" minOccurs="0"/>
            <xs:element name="WeeklyLateCharge" type="typeRateInfo" minOccurs="0" maxOccurs="1">
            <xs:annotation>
                  <xs:documentation>Extra Week Charges information. Supported providers:1P/1J only</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="PrintText" type="xs:string" minOccurs="0" maxOccurs="1">
               <xs:annotation>
                  <xs:documentation>Informational text provided by the supplier that may be related to the reservation. This is applicable in response messages only, 1p,1J only.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="RatePeriod" type="common:typeRateTimePeriod" use="optional">
            <xs:annotation>
               <xs:documentation>The period for the rate (daily, weekly, etc)</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="NumberOfPeriods" type="xs:int">
            <xs:annotation>
               <xs:documentation>Define how many periods (e.g. number of days or weeks)</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="UnlimitedMileage" type="xs:boolean" use="optional">
            <xs:annotation>
               <xs:documentation>True if unlimited miles for rate</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="MileageAllowance" type="xs:int" use="optional">
            <xs:annotation>
               <xs:documentation>Only set if UnlimitedMileage is false. Number of miles allowed for rate</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="Units" type="common:typeDistance" use="optional">
            <xs:annotation>
               <xs:documentation>Describes distance units for MileageAllowance or ExtraMileageCharge</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="RateSource" type="xs:string" use="optional">
            <xs:annotation>
               <xs:documentation>The rate source indicator for GWS</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="RateAvailability" type="typeRateAvailability" use="optional">
            <xs:annotation>
               <xs:documentation>Rate is available to sell, Need to Call or Closed</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="RequiredCharges" type="common:typeMoney"/>
         <xs:attribute name="RateCode" type="common:typeRateCode" use="optional">
         <xs:annotation>
               <xs:documentation>Rate Code of the vehicle. Supported Providers 1P,1J,1G,1V.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="RequestedRateCodeApplied" type="common:typeTrinary" use="optional">
         	 <xs:annotation>
               <xs:documentation>The requested Rate Code applied to the Rate. Valid values: "true", "false", "unknown". Supported Providers 1P,1J. </xs:documentation>
            </xs:annotation>
         </xs:attribute>         
         <xs:attribute name="RateCategory" type="common:typeRateCategory" use="optional">
            <xs:annotation>
               <xs:documentation>The category of this rate (Best, etc)</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="DiscountNumber" type="common:typeDiscountNumber" use="optional">
         	 <xs:annotation>
               <xs:documentation>Corporate Discount number used to request a specified discount. Supported Providers Requests: 1P,1J,1G,1V, Responses only 1P,1J. </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="DiscountNumberApplied" type="common:typeTrinary" use="optional">
         	 <xs:annotation>
               <xs:documentation>Discount Number has been applied to the Rate. Valid values: "true", "false", "unknown". Supported Providers 1P,1J. </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="VendorCode" type="common:typeSupplierCode" use="optional"/>
         <xs:attribute name="RateGuaranteed" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
               <xs:documentation>Guarantee indicator for vehicle rate.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="RateCodePeriod" type="xs:string" use="optional">
            <xs:annotation>
               <xs:documentation>RateCodePeriod</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="PromotionalCode" type="xs:string" use="optional">
            <xs:annotation>
               <xs:documentation>Specific coupon or promotion code. Providers 1P,1J,1V,1G.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="PromotionalCodeApplied" type="common:typeTrinary" use="optional">
         	 <xs:annotation>
               <xs:documentation>Promotional/Coupon Number has been applied to the Rate. Valid values: "true", "false", "unknown". Supported Providers 1P,1J. </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="TourCode" type="xs:string" use="optional">
            <xs:annotation>
               <xs:documentation>Tour Number for the Vehicle Booking</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="TourCodeApplied" type="common:typeTrinary" use="optional">
         	 <xs:annotation>
               <xs:documentation>Tour Code Number has been applied to the Rate. Valid values: "true", "false", "unknown". Supported Providers 1P,1J. </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="RateGuaranteeType" type="common:typeRateGuarantee" use="optional">
            <xs:annotation>
               <xs:documentation>To identify whether rate is already guaranteed or rate quoted or agent entered</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="RequiredPayment" use="optional">
            <xs:annotation>
               <xs:documentation>Returns Payment information required by vendor at the time og booking.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="Guarantee"/>
                  <xs:enumeration value="Deposit"/>
                  <xs:enumeration value="PrePayment"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="DropOffChargesIncluded" type="xs:boolean" use="optional">
            <xs:annotation>
               <xs:documentation>If true: Drop off charges are included. If false, not included. If not specified, additional drop off charges MAY apply (but this has not been confirmed by vendor)</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="CorporateRate" type="xs:boolean" use="optional">
            <xs:annotation>
               <xs:documentation>If "true" a Corporate Discount has been applied to the rate.  Applicable to 1P, 1J</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="AdvancedBooking" type="xs:string" use="optional">
            <xs:annotation>
               <xs:documentation>Indicates the number of Hours or Days a rate must be booked in advance. Values are for Days = number followed by “D” e.g., "002D" and Hours = number followed by “H” e.g., 002H” 1P/1J/1G/1V only.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="RentalRestriction" type="xs:boolean" use="optional">
            <xs:annotation>
               <xs:documentation>RentalRestriction attribute value is true if vehicle rate has rental restrictions. Rental restrictions can be obtained from the Vehicle Rules. 1P/1J only.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="FlightRestriction" type="xs:boolean" use="optional">
            <xs:annotation>
               <xs:documentation>Flight restriction indicator is true if flight information is required at booking. 1P/1J/1G/1V only.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="CardNumber" type="common:typeCardNumber" use="optional">
            <xs:annotation>
               <xs:documentation>Vehicle Loyalty Card Number. Supported Provider 1P,1J.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="CardNumberApplied" type="common:typeTrinary" use="optional">
            <xs:annotation>
               <xs:documentation>Loyalty Card Number has been applied to the Rate. Values: "true", "false", "unknown". Supported Providers 1P,1J.</xs:documentation>
            </xs:annotation>
         </xs:attribute> 
      </xs:complexType>
   </xs:element>
   
   	<xs:element name="SupplierRate">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="typeVehicleRates">
					<xs:attribute name="DiscountAmount" type="common:typeMoney"
						use="optional">
						<xs:annotation>
							<xs:documentation>Discount Amount 1P/1J only</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MandatoryChargeTotal" type="common:typeMoney" use="optional">
                        <xs:annotation>
                            <xs:documentation>Total Mandatory Charges, which may include taxes, surcharges, and fees. 1P, 1J only.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="typeVehicleRates">
      <xs:attribute name="EstimatedTotalAmount" type="common:typeMoney">
         <xs:annotation>
            <xs:documentation>The estimated total amount</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="BaseRate" type="common:typeMoney">
         <xs:annotation>
            <xs:documentation>Rate for the entire rent without the required charges</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="RateForPeriod" type="common:typeMoney">
         <xs:annotation>
            <xs:documentation>The rate for the period</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="DropOffCharge" type="common:typeMoney">
         <xs:annotation>
            <xs:documentation>The additional fee for dropping off a vehicle at a different location.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ExtraMileageCharge" type="common:typeMoney">
         <xs:annotation>
            <xs:documentation>Cost per mile over allowance for rate</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:complexType>
    <xs:element name="RateModifiers">
        <xs:complexType>
            <xs:attribute name="RateCode" use="optional" type="common:typeRateCode"/>
            <xs:attribute name="DiscountNumber" use="optional" type="common:typeDiscountNumber"/>
            <xs:attribute name="VendorCode" type="common:typeSupplierCode" use="optional"/>
            <xs:attribute name="PromotionalCode" type="xs:string" use="optional">
                <xs:annotation>
                    <xs:documentation>Specific coupon or promotion code</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="VendorLocationRef" type="xs:string" use="optional"/>
			<xs:attribute name="TourCode" type="xs:string" use="optional">
			   <xs:annotation>
			      <xs:documentation>Tour number or code.  Providers: 1P, 1J, 1G, 1V.</xs:documentation>
			   </xs:annotation>
			</xs:attribute>
        </xs:complexType>
    </xs:element>

    <xs:element name="VehicleSpecialRequest">
        <xs:annotation>
            <xs:documentation>Make a textual request to the Vehicle supplier.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="common:typeGeneralText">
                    <xs:attribute name="Key" type="common:typeRef" use="optional"/>
