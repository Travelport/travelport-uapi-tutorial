                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="TransmissionType" type="common:typeVehicleTransmission" use="optional">
                <xs:annotation>
                    <xs:documentation>Requested Transmission Type. Valid Values:  Automatic, Automatic4WD, ManualAWD, AutomaticAWD, Manual, Manual4WD.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attributeGroup ref="attrVehicleTypeInfo"/>
