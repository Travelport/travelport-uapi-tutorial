         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="VendorCode" type="xs:string" use="optional">
         <xs:annotation>
            <xs:documentation>The code of the vendor. For Air, Car and Hotel this will be an IATA vendor code.
                </xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="ProviderCode" type="xs:string" use="required"/>
      <xs:attribute name="ProviderLocatorCode" type="xs:string" use="required"/>
