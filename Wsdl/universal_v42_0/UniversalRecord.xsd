                use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>
                        Exclude Air reservations from the results
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ExcludeVehicle" type="xs:boolean"
                use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>
                        Exclude Vehicle reservations from the results
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ExcludeHotel" type="xs:boolean"
