           </xs:simpleType>
           </xs:attribute>
           <xs:attribute name="LastName">
              <xs:annotation>
                 <xs:documentation>Search with Traveler Last Name</xs:documentation>
              </xs:annotation>
                          <xs:simpleType>
              <xs:restriction base="xs:string">
                    <xs:maxLength value="30"/>
                    <xs:minLength value="1"/>
              </xs:restriction>
           </xs:simpleType>
           </xs:attribute>
        </xs:complexType>
