                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>

    <!--*** Shared Data Types ***-->
    
    <xs:complexType name="typeProductInfo">
      <xs:annotation>
         <xs:documentation>Information on the product type and its provider
            </xs:documentation>
      </xs:annotation>
      <xs:attribute name="ProductType" type="common:typeProduct" use="required">
