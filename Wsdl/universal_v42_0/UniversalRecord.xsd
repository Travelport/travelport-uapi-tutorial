        <xs:attribute name="VIPOnly" type="xs:boolean" use="optional">
           <xs:annotation>
              <xs:documentation>To Search for VIP traveler</xs:documentation>
           </xs:annotation>
        </xs:attribute>
     </xs:complexType>
  </xs:element>
  
  <xs:element name="NameCriteria">
       <xs:annotation>
           <xs:documentation>Traveler First Name and Last Name for Searching misc traveler information</xs:documentation>
       </xs:annotation>
