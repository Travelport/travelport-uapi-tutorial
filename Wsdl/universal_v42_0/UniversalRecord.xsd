   <xs:complexType name="typeDateSpec">
        <xs:annotation>
            <xs:documentation>Specifies dates as either specific date or a date range</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="DateRange" type="common:typeDateRange" minOccurs="0" maxOccurs="1"/>
