            <xs:enumeration value="No" />
            <xs:enumeration value="Not Applicable" />
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="typeExternalSearchIndex">
        <xs:annotation>
            <xs:documentation>
                This index is used for retrieving locator code from GDS.
                Provider specific info that can be used to help identify
                a record if no ProviderLocatorCode is known
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string" />
    </xs:simpleType>

    <xs:element name="SearchAgent">
        <xs:annotation>
            <xs:documentation>
