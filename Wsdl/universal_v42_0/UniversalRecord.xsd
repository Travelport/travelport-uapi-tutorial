        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="TicketStatus" type="common:URTicketStatus" use="optional">
         <xs:annotation>
            <xs:documentation>If the universal record is ticketed or not or partially ticketed </xs:documentation>
         </xs:annotation>
      </xs:attribute>
      </xs:complexType>
