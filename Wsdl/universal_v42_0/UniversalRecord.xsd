            <xs:documentation>SavedTrip holds a draft Itinerary/booking which can be used at later point of time to do a booking.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <!--  Common information. -->
                <xs:element ref="common:BookingTraveler" minOccurs="1" maxOccurs="unbounded"/>
                <xs:element ref="common:AgencyContactInfo" minOccurs="0"/>
                <xs:element ref="common:SearchPassenger" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="common:PointOfSale" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="common:AccountingRemark" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="common:GeneralRemark" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="common:AgencyInfo" minOccurs="0" />
                <xs:element ref="common:ReservationName" minOccurs="0" />
                
                <!--  Air related information -->
                <xs:choice>                    
                    <xs:element ref="air_v42_0:AirPricingModifiers" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice>                    
                    <xs:element ref="air_v42_0:AirPricingSolution" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:element name="AirTripNote" type="typeSavedTripNote" minOccurs="0" maxOccurs="9"/>
                <!--  Vehicle related information -->               
                <xs:choice>
                    <xs:element ref="vehicle_v42_0:VehicleSearchModifiers" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>                
                <xs:choice>
                    <xs:element ref="vehicle_v42_0:VehicleDateLocation" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>                
                
                <xs:element ref="common:SpecialEquipment" minOccurs="0" maxOccurs="unbounded"/>
                
                <xs:choice>
                    <xs:element ref="vehicle_v42_0:VehicleSpecialRequest" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice>
                    <xs:element ref="vehicle_v42_0:PaymentInformation" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice>
                    <xs:element ref="vehicle_v42_0:DeliveryAddress" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice>
                    <xs:element ref="vehicle_v42_0:CollectionAddress" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice>
                    <xs:element ref="vehicle_v42_0:FlightArrivalInformation" minOccurs="0" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:choice>
