            <xs:annotation>
               <xs:documentation>The date on which th earliest travel can occur
                    </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="CreatedDate" type="xs:string" use="optional">
            <xs:annotation>
               <xs:documentation>The date the resevation record was created
                    </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="SavedTripLocatorCode" type="common:typeLocatorCode" use="optional">
            <xs:annotation>
               <xs:documentation>Locator Code of the Saved Trip that is associated to the Universal Record.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
               <xs:attribute name="Ticketed" type="typeReservationTicketed" use="optional">
         <xs:annotation>
            <xs:documentation>If the universal record is ticketed or not or partially ticketed </xs:documentation>
         </xs:annotation>
