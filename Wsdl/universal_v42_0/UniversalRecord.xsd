                <xs:documentation>Determine if this segment is In or Out of
                    policy. By default it is InPolicy.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="InContract" type="xs:boolean" use="optional"
            default="true">
            <xs:annotation>
                <xs:documentation>Determine if this segment is In or Out of
                    contract. By default it is InContract.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
