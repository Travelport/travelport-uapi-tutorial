             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CreatingAgentDuty">
            <xs:annotation>
                <xs:documentation>
                    Duty code of the agent created the PNR
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CreatingAgencyIATA">
            <xs:annotation>
                <xs:documentation>
                    IATA number of the agency  created the PNR
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PrepaidTicketAdviceIndicator">
            <xs:annotation>
                <xs:documentation>
                    Indicates whether Prepaid Ticket Advice is attached to PNR
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BFBorrowed">
            <xs:annotation>
                <xs:documentation>
                    Indicates the current PNR as borrowed
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GlobalPNR">
            <xs:annotation>
                <xs:documentation>
                     Indicates whether current PNR is Global PNR
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ReadOnlyPNR">
            <xs:annotation>
                <xs:documentation>
                    Indicates PNR as read only
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PastDateQuickRetrievedPNR">
            <xs:annotation>
                <xs:documentation>
                     Indicates PNR retrieved from offline archival process                 
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SuperPNR">
            <xs:annotation>
                <xs:documentation>
                    Indicates whether current PNR is Super PNR
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PNRPurgeDate">
            <xs:annotation>
                <xs:documentation>
                    Purge date of the PNR
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="OriginalReceivedFieldValue">
            <xs:annotation>
                <xs:documentation>
                    The original data in the received field
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DivPsgrName">
            <xs:annotation>
                <xs:documentation>
                    Passenger Name in a divided PNR
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TruncInd">
            <xs:annotation>
                <xs:documentation>
                    Indicator for Truncated names
                </xs:documentation>     
             </xs:annotation>            
         </xs:enumeration>
         <xs:enumeration value="DivTypeInd">
            <xs:annotation>
                <xs:documentation>
                    Divide Type Indicator if it pertains to child or parent booking
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RLoc">
            <xs:annotation>
                <xs:documentation>
                    Record Locator of parent or child booking
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DivDt">
            <xs:annotation>
                <xs:documentation>
                    Date of divide action
                </xs:documentation>     
             </xs:annotation>            
         </xs:enumeration>
         <xs:enumeration value="DivTm">
            <xs:annotation>
                <xs:documentation>
                    Time of day of divide action
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TravelOrder">
            <xs:annotation>
                <xs:documentation>
                    Travel order of the segment
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SegmentStatus">
            <xs:annotation>
                <xs:documentation>
                    Status of associated segment
                </xs:documentation>     
             </xs:annotation>            
         </xs:enumeration>
         <xs:enumeration value="StartDate">
            <xs:annotation>
                <xs:documentation>
                     Date of departure of the rail segment
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DayChange">
            <xs:annotation>
                <xs:documentation>
                    Indicates arrival date as number of days before or after departure date
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Vendor">
            <xs:annotation>
                <xs:documentation>
                        Vendor code of the segment
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StartTime">
            <xs:annotation>
                <xs:documentation>
                    Start Time of the segment
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="EndTime">
            <xs:annotation>
                <xs:documentation>
                        End Time of the segment
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BookingCode">
            <xs:annotation>
                <xs:documentation>
                   The booking code of the rail segment seating area
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TrainNumber">
            <xs:annotation>
                <xs:documentation>
                    Denotes Train number
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="NumberOfSeats">
            <xs:annotation>
                <xs:documentation>
                    Number of seats sold for the trip
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SellType">
            <xs:annotation>
                <xs:documentation>
                   The mode of selling the segment
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="TariffType">
            <xs:annotation>
                <xs:documentation>
                     Type of Tariff for the segment
                </xs:documentation>     
             </xs:annotation>            
         </xs:enumeration>
         <xs:enumeration value="ConfirmationNumber">
            <xs:annotation>
                <xs:documentation>
                     The confirmation number of the segment
                </xs:documentation>     
             </xs:annotation>            
         </xs:enumeration>
         <xs:enumeration value="BoardingInformation">
            <xs:annotation>
                <xs:documentation>
                     Information related to boarding point of the segment
                </xs:documentation>     
             </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ArrivalInformation">
            <xs:annotation>
                <xs:documentation>
                   Information related to arrival point of the segment
                </xs:documentation>     
             </xs:annotation>            
         </xs:enumeration>
         <xs:enumeration value="Text">
            <xs:annotation>
                <xs:documentation>
                    Additional   information
                </xs:documentation>     
             </xs:annotation>            
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
    
    <xs:element name="DisplayDetails">
        <xs:annotation>
            <xs:documentation>The container to display the contents of PNR in GDS format.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:choice>
                    <xs:element ref="DisplayDetail" maxOccurs="unbounded"/>
                    <xs:element ref="DisplayContents"/>
                </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="attrProviderReservationDetails"/>
        </xs:complexType>
   </xs:element>
   <xs:element name="TravelerNameNum">
        <xs:annotation>
            <xs:documentation>Container for Host Passenger Details.Each TravelerNameNum represents a passenger present in the PNR. This is currently used by JAL(1J) only.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attributeGroup ref="common:attrBookingTravelerName" />
            <xs:attribute name="PaxNum" type="xs:string" use="required">
                <xs:annotation>
                    <xs:documentation>Passenger Name Number in host. Should be in the format SurNameNum.GivenNameNum</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
   
    <xs:element name="DisplayContents" type="xs:string">
        <xs:annotation>
            <xs:documentation>Display the contents in GDS format</xs:documentation>
        </xs:annotation>
    </xs:element>
   
    <xs:element name="DisplayDetail">
        <xs:annotation>
            <xs:documentation>Display the contents for requested MCO,Cruise etc. details</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="Name" type="typeDisplayDetailName" use="required"/>
            <xs:attribute name="Value" type="xs:string" use="required"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="QueueNextModifiers">
         <xs:annotation>
                <xs:documentation>Can only be used when modifying an Universal Record in Queue mode.If not
specified along with ReturnRecord as false then current PNR in queue context
will be removed.</xs:documentation>
         </xs:annotation>
          <xs:complexType>
               <xs:attribute name="NextOnQueue" type="xs:boolean">
                 <xs:annotation>
                            <xs:documentation>Set to true to retrieve the next PNR on Queue ,if not set or set to false system would return the current PNR.NextOnQueue cannot be combined with Provider Locator Code and ReturnRecord as true</xs:documentation>
                      </xs:annotation>
               </xs:attribute>       
               <xs:attribute name="ProviderLocatorCode" type="common:typeLocatorCode" use="optional">
                  <xs:annotation>
                               <xs:documentation>If providerLocatorCode is specified then system would return the specified locator code in Queue mode .Provider Locator Code cannot be combined with NextOnQueue and ReturnRecord as true </xs:documentation>
                  </xs:annotation>
                </xs:attribute>
                <xs:attribute name="ReQueueCurrent" type="xs:boolean">
                      <xs:annotation>
                            <xs:documentation>Set to true to place the current PNR back on Queue</xs:documentation>
                      </xs:annotation>
                </xs:attribute>
                <!-- Added new attribute Queue Session Token for story 895  -->
                <xs:attribute name="QueueSessionToken" type="xs:string" use="optional">
                  		<xs:annotation>
                     		<xs:documentation>Queue Session Token to hold session token for multiple queue</xs:documentation>
                  		</xs:annotation>
               			</xs:attribute>
                     
            </xs:complexType>
     </xs:element>  
     
     <xs:element name="UniversalModifyFailureInfo">
       <xs:annotation>
           <xs:documentation>Container to return air segment sell failures.</xs:documentation>
       </xs:annotation>
       <xs:complexType>
           <xs:sequence>
               <xs:element ref="UniversalModifyCommandError" minOccurs="0" maxOccurs="unbounded"/>
               <xs:element ref="air_v42_0:AirSegmentSellFailureInfo" minOccurs="0"/>
           </xs:sequence>
       </xs:complexType>
   </xs:element>
 
   <xs:element name="UniversalModifyCommandError">
       <xs:annotation>
           <xs:documentation>Container to return modify command failures.</xs:documentation>
       </xs:annotation>
       <xs:complexType>
           <xs:simpleContent>
               <xs:extension base="xs:string">
                   <xs:attribute name="UniversalModifyCmdKey" type="common:typeRef" use="required"/>
               </xs:extension>
           </xs:simpleContent>
       </xs:complexType>
   </xs:element>
   
   <xs:element name="UniversalModifyErrorInfo">
        <xs:annotation>
           <xs:documentation>Container to return modify command errors.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:typeErrorInfo">
                    <xs:sequence>
                        <xs:element ref="UniversalModifyCommandError" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="air_v42_0:AirSegmentError" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
   </xs:element>
   
   <xs:simpleType name="typeRetainReservation">
        <xs:annotation>
            <xs:documentation>Retain the Reservation (do not cancel) in the
                the event of a schedule or price change</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="None"/>
            <xs:enumeration value="Schedule"/>
            <xs:enumeration value="Price"/>
            <xs:enumeration value="Both"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:element name="ProviderReservationDisplayDetailsList">
        <xs:annotation>
            <xs:documentation>
            Response to display the addtional details of provider reservation information  .
         </xs:documentation>
        </xs:annotation>
        <xs:complexType>
	        <xs:sequence>
	            <xs:element ref="DisplayDetails" maxOccurs="unbounded" />
	            <xs:element ref="TravelerNameNum" minOccurs="0" maxOccurs="unbounded"/>
	        </xs:sequence>
        </xs:complexType>
    </xs:element>
    
</xs:schema>
