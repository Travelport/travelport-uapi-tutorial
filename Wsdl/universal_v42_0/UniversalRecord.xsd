                    <xs:documentation>
                        The Agent ID that created a PNR.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ModifiedByAgent" type="xs:string"
                use="optional">
                <xs:annotation>
                    <xs:documentation>
                        The Agent ID that modified a PNR last.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="TicketedByAgent" type="xs:string"
                use="optional">
                <xs:annotation>
                    <xs:documentation>
                        The Agent ID that ticketed a PNR.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="SearchAccount">
        <xs:annotation>
            <xs:documentation>
                Search restriction by Account
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="ClientID" type="xs:string"
                use="optional">
                <xs:annotation>
                    <xs:documentation>
                        Identifier of the corporation this account is
                        for.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="BranchID" type="common:typeBranchId"
                use="optional">
                <xs:annotation>
                    <xs:documentation>
                        Account Branch ID.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="BranchId" type="xs:string">
        <xs:annotation>
            <xs:documentation>
                Branch ID to restrict search.
            </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="UniversalRecordHistorySearchResult">
        <xs:annotation>
            <xs:documentation>
                Container for Universal Record history result.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Old" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>
                            Representation of the element before the
                            modification took place.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="New" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>
                            Representation of the element after the
                            modification took place.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="ModifiedBy" type="xs:string"
                use="optional">
                <xs:annotation>
                    <xs:documentation>
                        The agent or entity that performed the
                        modification
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ModifiedDate" type="xs:string"
                use="required">
                <xs:annotation>
                    <xs:documentation>
                        When the Universal Record was modified.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ElementType" type="xs:string"
                use="required">
                <xs:annotation>
                    <xs:documentation>
                        The type of element that was added or deleted
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Action" type="xs:string"
                use="required">
                <xs:annotation>
                    <xs:documentation>
                        The action that was taken on the defined element
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="TransactionId" type="xs:string" use="optional">
                <xs:annotation>
                     <xs:documentation>
                        System generated unique identifier for this atomic transaction. Travelport Internal Usage Only.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
        	<xs:attributeGroup ref="common:attrAgentOverride"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="UniversalRecordHistorySearchModifiers">
        <xs:annotation>
            <xs:documentation>
                Controls and switches for the Universal Record history
                request.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="ElementType" type="xs:string"
                    minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>
                            The Type of History that is to be searched
                            for.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:choice minOccurs="0">
                    <xs:element name="ModifiedDate" type="xs:date">
                        <xs:annotation>
                            <xs:documentation>
                                Only search for modifications that
                                occured on that date.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ModifiedRange">
                        <xs:annotation>
                            <xs:documentation>
                                Only search for modifications that
                                occured between the two dates
                                (inclusive).
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="ModifiedStart"
                                    type="xs:date" />
                                <xs:element name="ModifiedEnd"
                                    type="xs:date" />
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:choice>
            </xs:sequence>
           <xs:attribute name="MaxResults" type="common:typeMaxResults"
                use="optional" default="20"/>
            <xs:attribute name="StartFromResult" type="common:typeStartFromResult" use="optional"
                default="1"/>
            <xs:attribute name="ModifiedBy">
                <xs:annotation>
                    <xs:documentation>
                        Agent code that modified the Universal Record
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DebugMode" type="xs:boolean" use="optional" default="false">
                <xs:annotation>
                     <xs:documentation>
                        Returns Debug info. Travelport Internal Usage Only.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="SegmentContinuityInfo">
        <xs:annotation>
            <xs:documentation>This container holds Arnks and segment continuity remarks</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ArvlUnknSegment" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="ContinuityOverrideRemark" minOccurs="0" maxOccurs="unbounded" />    
            </xs:sequence>
            <xs:attribute name="ArrivalUnknownSegmentCount" type="xs:integer" use="optional"/>
        </xs:complexType>
    </xs:element> 
       
    <xs:element name="ArvlUnknSegment">
        <xs:annotation>
            <xs:documentation>An ARNK segment that identifies a missing travel information</xs:documentation>
        </xs:annotation>
        <xs:complexType>
                <xs:sequence>
                     <xs:element name="BookingTravelerRef" minOccurs="0" maxOccurs="255">
                         <xs:annotation>
                            <xs:documentation>Reference Element for Booking Traveler</xs:documentation>
                        </xs:annotation>
                         <xs:complexType>                        
                            <xs:attribute name="Key" type="common:typeRef" use="required"/>
                        </xs:complexType>
                     </xs:element>
                </xs:sequence>
                <xs:attribute name="Key" type="common:typeRef" use="required"/>
                <xs:attribute name="Origin" type="common:typeIATACode" use="optional">
                    <xs:annotation>
                        <xs:documentation>
                            The IATA CITY code for this origination of this entity.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="Destination" type="common:typeIATACode" use="optional">
                    <xs:annotation>
                        <xs:documentation>
                            The IATA CITY code for this destination of this entity.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>    
                <xs:attribute name="TravelOrder" type="xs:integer" use="optional">
                <xs:annotation>
                    <xs:documentation>To identify the appropriate travel sequence for Air/Car/Hotel segments/reservations based on travel dates. This ordering is applicable across the UR not provider or traveler specific</xs:documentation>
                </xs:annotation>
                </xs:attribute>                                
        </xs:complexType>        
    </xs:element>
   
    <xs:element name="ContinuityOverrideRemark">
        <xs:annotation>
            <xs:documentation>A textual remark container to hold any printable text. (max 512 chars)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="Category" use="optional" default="MCT">
                    <xs:annotation>
                            <xs:documentation>This is remark category is always MCT. 'Minimum Connect Time'</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="10"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="BaseSearchModifiers">
      <xs:annotation>
      <xs:documentation>Controls and switches for the Universal Record Search and Saved Trip search request.</xs:documentation>
      </xs:annotation>
        <xs:sequence>
            <xs:element name="TravelDate" type="typeDateSpec" minOccurs="0">
               <xs:annotation>
                  <xs:documentation>Matched with flight departure date or hotel check-in date or vehicle pick-up date. For Rail, travel date should be between journey start and end dates, Cannot be combined with any date in product level ReservationCriteria</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="IncludeAllNames" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
               <xs:documentation>
                        If true, include all the passenger names in the results. Otherwise include only the name of the primary passenger.
                    </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="IncludeAgentInfo" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
               <xs:documentation>
                        Include information about the agent who created, modified or ticketed the Universal Record.
                    </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="MaxResults" use="optional" type="common:typeMaxResults" default="20"/>
         <xs:attribute name="StartFromResult" use="optional" type="common:typeStartFromResult"/>
         <xs:attribute name="ExcludeAir" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
               <xs:documentation>
                        Exclude Air reservations from the results
                    </xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="ExcludeVehicle" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
               <xs:documentation>Exclude Vehicle reservations from the results</xs:documentation>
            </xs:annotation>
         </xs:attribute>
         <xs:attribute name="ExcludeHotel" type="xs:boolean" use="optional" default="false">
            <xs:annotation>
               <xs:documentation>Exclude Hotel reservations from the results.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
    </xs:complexType>
    <xs:element name="UniversalRecordSearchModifiers">
      <xs:annotation>
         <xs:documentation>Controls and switches for the Universal Record Search request.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
        <xs:complexContent>
            <xs:extension base="BaseSearchModifiers"/>
        </xs:complexContent>
      </xs:complexType>
   </xs:element>
   
     <xs:element name="TravelerCriteria"> 
     <xs:annotation>
        <xs:documentation>Criteria for Searching misc traveler information</xs:documentation>
     </xs:annotation>
     <xs:complexType>
       <xs:choice>
          <xs:annotation>
              <xs:documentation>Specifies Search Criteria as either Travler Name Details or Traveler Applied Profile Id</xs:documentation>
          </xs:annotation>
           <xs:element ref="NameCriteria" minOccurs="0" />
           <xs:element ref="AppliedProfileCriteria" minOccurs="0"/>
