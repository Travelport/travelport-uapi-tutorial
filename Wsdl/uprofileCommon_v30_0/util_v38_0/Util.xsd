                        <xs:element name="TaxCategory" minOccurs="0"
                            maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="Code"
                                    type="xs:string" use="required" />
                                <xs:attribute name="Description"
                                    type="xs:string" use="optional" />
