    <!--*** Common Reference Elements ***-->
    <xs:element name="CreditCardPaymentAuth">
        <xs:complexType>
            <xs:choice>
                <xs:element ref="common:CreditCard" minOccurs="0" />
                <xs:element ref="common:FormOfPaymentRef" minOccurs="0" />
            </xs:choice>
            <xs:attribute name="SecurityCode" use="optional">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="3" />
                        <xs:maxLength value="5" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="Amount" type="common:typeMoney"
                use="required" />
            <xs:attribute name="PerformAVS" type="xs:boolean"
                use="required" />
        </xs:complexType>
    </xs:element>

    <xs:element name="CurrencyConversion">
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>
                    Currency Conversion Entry Parameters
                </xs:documentation>
            </xs:annotation>
            <xs:attribute name="From" type="common:typeCurrency"
                use="required">
                <xs:annotation>
                    <xs:documentation>
                        From Currency Value for the Conversion
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="To" type="common:typeCurrency"
                use="required">
                <xs:annotation>
                    <xs:documentation>
                        To Currency Value for the Conversion
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="OriginalAmount" type="xs:float"
                use="optional">
                <xs:annotation>
                    <xs:documentation>
                        Amount to be converted
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ConvertedAmount" type="xs:float"
                use="optional">
                <xs:annotation>
                    <xs:documentation>
                        Amount after the convertion
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="BankSellingRate" type="xs:float"
                use="optional">
                <xs:annotation>
                    <xs:documentation>
                        Currency Conversion Rate
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>

    <xs:element name="McoCreateDate" type="common:typeTimeRange" />
    <xs:element name="McoSearchModifiers">
        <xs:annotation>
            <xs:documentation>
                Modifiers to narrow the MCO search results.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="Type" type="common:typeMCOType"
                use="optional">
                <xs:annotation>
                    <xs:documentation>
                        Find all MCOs of a particular type
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Status" type="common:typeMCOStatus"
                use="optional">
                <xs:annotation>
                    <xs:documentation>
                        Find all MCOs of a particular status.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MaxResults" use="optional"
                default="20">
                <xs:annotation>
                    <xs:documentation>
                        Used to limit the number of results returned,
                        particularly in more 200809 searches that may
                        return a large result set.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:minInclusive value="1" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="StartFromResult" use="optional"
                default="0">
                <xs:annotation>
                    <xs:documentation>
                        Used to browse beyond the maximum number of
                        results specified with the MaxResults parameter.
                        Acts as an offset to skip the specified number
                        of items from the begining of the result set.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:minInclusive value="0" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="IncludeName" type="xs:boolean"
                use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>
                        Should the McoSearchResult include the name on
                        the MCO
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="McoSearchResult">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="common:Name" minOccurs="0" />
            </xs:sequence>
            <xs:attribute name="CreateDate" type="xs:dateTime"
                use="required">
                <xs:annotation>
                    <xs:documentation>
                        The date the MCO was issued
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Number" type="xs:string"
                use="required">
                <xs:annotation>
                    <xs:documentation>The MCO number</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Status" type="common:typeMCOStatus"
                use="required">
                <xs:annotation>
                    <xs:documentation>
                        The status of the MCO
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Type" type="common:typeMCOType"
                use="required">
                <xs:annotation>
                    <xs:documentation>
                        The Type of the MCO
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="LocatorCode" type="xs:string"
                use="optional">
                <xs:annotation>
                    <xs:documentation>
                        The locator code that the MCO is linked to
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="MctSearch">
        <xs:annotation>
            <xs:documentation>
                Search the MCT data for exceptions
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="ArriveStation"
                type="common:typeIATACode" use="required" />
            <xs:attribute name="DepartStation"
                type="common:typeIATACode" use="optional" />
            <xs:attribute name="Connection" type="typeMctConnection"
                use="optional" />
            <xs:attribute name="ArriveCarrier" type="common:typeCarrier"
                use="optional" />
            <xs:attribute name="DepartCarrier" type="common:typeCarrier"
                use="optional" />
            <xs:attribute name="ArriveFlight" type="xs:string"
                use="optional" />
            <xs:attribute name="DepartFlight" type="xs:string"
                use="optional" />
            <xs:attribute name="PreviousStation"
                type="common:typeIATACode" use="optional" />
            <xs:attribute name="NextStation" type="common:typeIATACode"
                use="optional" />
            <xs:attribute name="PreviousCountry"
                type="common:typeCountry" use="optional" />
            <xs:attribute name="NextCountry" type="common:typeCountry"
                use="optional" />
            <xs:attribute name="PreviousState" type="common:typeState"
                use="optional" />
            <xs:attribute name="NextState" type="common:typeState"
                use="optional" />
            <xs:attribute name="TravelDate" type="xs:date"
                use="optional" />
        </xs:complexType>
    </xs:element>
    <xs:element name="MctQuery">
        <xs:annotation>
            <xs:documentation>
                Lookup the particular MCT time between two segments
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="air:AirSegment" minOccurs="2"
                    maxOccurs="2" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="MctException">
        <xs:annotation>
            <xs:documentation>MCT Exception details</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="Time" type="xs:int" use="required" />
            <xs:attribute name="ArriveStation"
                type="common:typeIATACode" use="required" />
            <xs:attribute name="DepartStation"
                type="common:typeIATACode" use="optional" />
            <xs:attribute name="Connection" type="typeMctConnection"
                use="required" />
            <xs:attribute name="ArriveCarrier" type="common:typeCarrier"
                use="optional" />
            <xs:attribute name="DepartCarrier" type="common:typeCarrier"
                use="optional" />
            <xs:attribute name="ArriveFlightRangeBegin" type="xs:string"
                use="optional" />
            <xs:attribute name="ArriveFlightRangeEnd" type="xs:string"
                use="optional" />
            <xs:attribute name="DepartFlightRangeBegin" type="xs:string"
                use="optional" />
            <xs:attribute name="DepartFlightRangeEnd" type="xs:string"
                use="optional" />
            <xs:attribute name="ArriveEquipment" type="xs:string"
                use="optional" />
            <xs:attribute name="DepartEquipment" type="xs:string"
                use="optional" />
            <xs:attribute name="PreviousStation"
                type="common:typeIATACode" use="optional" />
            <xs:attribute name="NextStation" type="common:typeIATACode"
                use="optional" />
            <xs:attribute name="PreviousCountry"
                type="common:typeCountry" use="optional" />
            <xs:attribute name="NextCountry" type="common:typeCountry"
                use="optional" />
            <xs:attribute name="PreviousState" type="common:typeState"
                use="optional" />
            <xs:attribute name="NextState" type="common:typeState"
                use="optional" />
            <xs:attribute name="ArriveTerminal" type="xs:string"
                use="optional" />
            <xs:attribute name="DepartTerminal" type="xs:string"
                use="optional" />
            <xs:attribute name="EffectiveDate" type="xs:date"
                use="optional" />
            <xs:attribute name="DiscontinueDate" type="xs:date"
                use="optional" />
        </xs:complexType>
    </xs:element>
    <xs:element name="MctCount">
        <xs:annotation>
            <xs:documentation>
                The count of MCT exceptions for the given search
                criteria
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="Connection" type="typeMctConnection"
                use="required" />
            <xs:attribute name="Carrier" type="common:typeCarrier"
                use="optional" />
            <xs:attribute name="Count" type="xs:int" use="required" />
        </xs:complexType>
    </xs:element>
    <xs:element name="MctStandard">
        <xs:annotation>
            <xs:documentation>
                The standard MCT time for the given search criteria
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="Type" type="typeMctConnection"
                use="required" />
            <xs:attribute name="Time" type="xs:int" use="required" />
        </xs:complexType>
    </xs:element>

    <!--*** Common Type ***-->
    <xs:simpleType name="typeMctConnection">
        <xs:annotation>
            <xs:documentation>
                Enumeration of the types of MCT exceptions (domestic or
                international)
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="DD" />
            <xs:enumeration value="DI" />
            <xs:enumeration value="ID" />
            <xs:enumeration value="II" />
        </xs:restriction>
    </xs:simpleType>
    <!--  Tax Service Elements -->
    <xs:element name="TaxCalcInfo">
        <xs:annotation>
            <xs:documentation>
                Container for a single segment for tax calculation
                purposes
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="Origin" type="common:typeIATACode"
                use="required" />
            <xs:attribute name="Destination" type="common:typeIATACode"
                use="required" />
            <xs:attribute name="BaseFare" type="common:typeMoney"
                use="required" />
            <xs:attribute name="QSurcharge" type="common:typeMoney"
                use="optional" />
            <xs:attribute name="StopOverFee" type="common:typeMoney"
                use="optional" />
            <xs:attribute name="Tax" type="common:typeMoney"
                use="optional" />
        </xs:complexType>
    </xs:element>
    <xs:element name="CalculateTaxResult">
        <xs:annotation>
            <xs:documentation>
                Result container for a tax calculation
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="TaxCalcInfo" minOccurs="0"
                    maxOccurs="unbounded" />
            </xs:sequence>
            <xs:attribute name="TotalBaseFare" type="common:typeMoney"
                use="required" />
            <xs:attribute name="TotalTax" type="common:typeMoney"
                use="required" />
            <xs:attribute name="TotalFare" type="common:typeMoney"
                use="required" />
        </xs:complexType>
    </xs:element>
    <xs:element name="CalculateTaxReq">
        <xs:annotation>
            <xs:documentation>
                Request to calculate US taxes based on a series of
                segments
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="TaxCalcInfo" minOccurs="0"
                            maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="CalculateTaxRsp">
        <xs:annotation>
            <xs:documentation>
                Response containg calculated total of base prices and
                taxes
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="CalculateTaxResult" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="ContentProviderRetrieveReq">
        <xs:annotation>
            <xs:documentation>
                Retrieves all available providers with their suppliers.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:attribute name="ProviderCode" type="common:typeProviderCode" use="optional">
                        <xs:annotation>
                            <xs:documentation>If ProviderCode present, Retruns Data for the ProviderCode</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="SupplierCode" type="common:typeSupplierCode" use="optional">
                        <xs:annotation>
                            <xs:documentation>Represents SupplierCode or Carrier code. If SupplierCode Presents, returns Data for the SupplierCode.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="ShowProviders"
                        type="typeShowProvidersType" default="Provisioned">
                        <xs:annotation>
                            <xs:documentation>
                                Enumeration of reqested type of Provider
                                Configuration requested - default is
                                "Provisioned". An error may be returned
                                if 'All' is requested and the user
                                security level is not allowed this
                                access
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="ContentProviderRetrieveRsp">
        <xs:annotation>
            <xs:documentation>
                Response with all available providers with their
                suppliers.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="ContentProvider" minOccurs="0"
                            maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="ContentProvider">
        <xs:annotation>
            <xs:documentation>
                A content provider uniquely identified with its provider
                code and supplier code.If a given provider provides
                content for multiple suppliers, multiple elements will
                be returned for that provider.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="ProviderCode" type="xs:string"
                use="required">
                <xs:annotation>
                    <xs:documentation>
                        The host for the content
                        (e.g.1G,1P,1S,1A,ACH,MCH).
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="SupplierCode" type="xs:string"
                use="optional">
                <xs:annotation>
                    <xs:documentation>
                        Indicates the direct connect supplier (e.g. U2)
                        when the provider is a hub (e.g. ACH).
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="AgencyCredentials" use="required">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether agency (or agent ) credentials
                        are required to connect to the given
                        provider/supplier.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Required" />
                        <xs:enumeration value="Optional" />
                        <xs:enumeration value="Prohibited" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="Active" type="xs:boolean"
                use="required">
                <xs:annotation>
                    <xs:documentation>
                        Status of the given provider/supplier . If not
                        currently active, a user cannot connect to the
                        given provider/supplier.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Provisionable" type="xs:boolean"
                use="required">
                <xs:annotation>
                    <xs:documentation>
                        Indicates whether the given provider/supplier
                        can be provisioned to an agency/branch/agent.
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MerchandisingACHAdapter" type="xs:string">
                <xs:annotation>
                    <xs:documentation>ACH adapter for Merchandising ACH carrier.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="StaticDataCarrier" type="xs:boolean" use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>Returns "true" if it is StaticDataCarrier otherwise "false". Default value is "false".</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MerchandisingACHCarrier" type="xs:boolean" use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>Returns "true" if it is MerchandisingACHCarrier otherwise "false". Default value is "false".</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MerchandisingHubCarrier" type="xs:boolean" use="optional" default="false">
                <xs:annotation>
                    <xs:documentation>Returns "true" if it is MerchandisingHubCarrier otherwise "false". Default value is "false".</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attributeGroup ref="attrProviderSupplierCapabilities" />
        </xs:complexType>
    </xs:element>
    
    <xs:element name="ReferenceDataItem">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="AdditionalInfo" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:documentation>Any additional information specific to a type of value being returned.</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:simpleContent>
                     <xs:extension base="common:StringLength1to255">
                        <xs:attribute name="Type" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>This will identify different additionalInfo.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:extension>
                  </xs:simpleContent>
               </xs:complexType>
            </xs:element>
         </xs:sequence>
         <xs:attributeGroup ref="attrReferenceDataItem"/>
         <xs:attribute name="Deprecated" type="xs:boolean" use="required">
            <xs:annotation>
               <xs:documentation>Indicates if the reference data item is deprecated.</xs:documentation>
            </xs:annotation>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   
    <xs:attributeGroup name="attrReferenceDataItem">
      <xs:attribute name="Code" type="common:StringLength1to128" use="required">
         <xs:annotation>
            <xs:documentation>The code of the reference data item.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Name" type="common:StringLength1to128">
         <xs:annotation>
            <xs:documentation>The name of the reference data item.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Description" type="common:StringLength1to255">
         <xs:annotation>
            <xs:documentation>The description of the reference data item.</xs:documentation>
         </xs:annotation>
      </xs:attribute>
   </xs:attributeGroup>
    
    <xs:element name="ReferenceDataSearchModifiers">
        <xs:annotation>
            <xs:documentation>
                Modifiers to narrow the reference data search results.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
        	<xs:sequence>
            	<xs:element ref="RailStationLocationModifiers" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute name="MaxResults" use="optional"
                default="20">
                <xs:annotation>
                    <xs:documentation>
                        Used to limit the number of results returned.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:minInclusive value="1" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="StartFromResult" use="optional"
                default="0">
                <xs:annotation>
                    <xs:documentation>
                        Used to browse beyond the maximum number of
                        results specified with the MaxResults parameter.
                        Acts as an offset to skip the specified number
                        of items from the begining of the result set.
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:integer">
                        <xs:minInclusive value="0" />
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="ProviderCode" type="common:typeProviderCode" use="optional">
	            <xs:annotation>
	               <xs:documentation>Provider Specific restriction(e.g. 1G, 1P etc) .</xs:documentation>
	            </xs:annotation>
	       </xs:attribute>
        </xs:complexType>
    </xs:element>

    <xs:element name="ReferenceDataSearchItem">
        <xs:annotation>
            <xs:documentation>
                Container for a Reference Data search item that includes
                its type and how to search / identify the item either
                via code or value.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice>
                <xs:element name="Code" minOccurs="0" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>
                            Code uniquely identifying the reference data
                            item (e.g. ORD for Airport item).
                        </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:whiteSpace value="collapse" />
                            <xs:minLength value="1" />
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="Name" minOccurs="0" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>
                            Value of the reference data item that may
                            not uniquely identify a single Reference
                            Data item (e.g. Chicago for Airport would
                            return ORD and MDW).
                        </xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:whiteSpace value="collapse" />
                            <xs:minLength value="1" />
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
            </xs:choice>
            <xs:attribute name="Type" use="required">
                <xs:annotation>
                    <xs:documentation>
                        Reference data type
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Airport" />
                        <xs:enumeration value="Carrier" />
                        <xs:enumeration value="City" />
                        <xs:enumeration value="Country" />
                        <xs:enumeration value="Currency" />
                        <xs:enumeration value="Equipment" />
                        <xs:enumeration value="PassengerType" />
                        <xs:enumeration value="SsrType" />
                        <xs:enumeration value="State" />
                        <xs:enumeration value="TaxCategory" />
                        <xs:enumeration value="FulfillmentType" />                        
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>

    <xs:simpleType name="typeEmailRecipientType">
        <xs:annotation>
            <xs:documentation>
                Enumeration of the types of email recipients available
                for use (TO, CC, BCC)
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="TO" />
            <xs:enumeration value="CC" />
            <xs:enumeration value="BCC" />
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="typeEmailAddress">
        <xs:sequence>
            <xs:element ref="common:Email" />
            <xs:element ref="common:SimpleName" minOccurs="0" />
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="typeShowProvidersType">
        <xs:annotation>
            <xs:documentation>
                Enumeration of reqested type of Provider Configuration
                requested. An error may be returned if 'All' and the
                user security level is not allowed this access
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="All" />
            <xs:enumeration value="Provisioned" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="typeProviderSupplierCapabilityType">
        <xs:annotation>
            <xs:documentation>
                Enumeration of ProviderSupplier capability
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Yes" />
            <xs:enumeration value="No" />
            <xs:enumeration value="Partial" />
        </xs:restriction>
    </xs:simpleType>

    <xs:attributeGroup name="attrProviderSupplierCapabilities">
        <xs:annotation>
            <xs:documentation>
                Indications of Provider and/or Supplier capabilities.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="BookingRetrieve"
            type="typeProviderSupplierCapabilityType" use="optional"
            default="Yes">
            <xs:annotation>
                <xs:documentation>
                    Indication if a Provider and/or Supplier has booking
                    retrieve capability.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SegmentModify"
            type="typeProviderSupplierCapabilityType" use="optional"
            default="Yes">
            <xs:annotation>
                <xs:documentation>
                    Indication if a Provider and/or Supplier has booking
                    segment modify capability.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="OptionalServicesModify"
            type="typeProviderSupplierCapabilityType" use="optional"
            default="Yes">
            <xs:annotation>
                <xs:documentation>
                    Indication if a Provider and/or Supplier has booking
                    optional service modify capability.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="TravelerInfoModify"
            type="typeProviderSupplierCapabilityType" use="optional"
            default="Yes">
            <xs:annotation>
                <xs:documentation>
                    Indication if a Provider and/or Supplier has
                    traveler information modify capability.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="AdditionalPayment"
            type="typeProviderSupplierCapabilityType" use="optional"
            default="Yes">
            <xs:annotation>
                <xs:documentation>
                    Indication if a Provider and/or Supplier has
                    additional payment capability.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="BookingCancel"
            type="typeProviderSupplierCapabilityType" use="optional"
            default="Yes">
            <xs:annotation>
                <xs:documentation>
                    Indication if a Provider and/or Supplier has booking
                    cancel capability.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SeatMap"
            type="typeProviderSupplierCapabilityType" use="optional"
            default="Yes">
            <xs:annotation>
                <xs:documentation>
                    Indication if a Provider and/or Supplier has SeatMap
                    capability.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    
