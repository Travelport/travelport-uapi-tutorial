   <xs:element name="FareFamilyUpdate">
      <xs:annotation>
         <xs:documentation>Branded fare admin request element to Update a FareFamily.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="FareFamily"/>
         </xs:sequence>
      </xs:complexType>
