   <xs:complexType name="typeFlightSpec">
      <xs:annotation>
         <xs:documentation>
            Specifies flight number as either specific flight number
            or a flight number range
         </xs:documentation>
      </xs:annotation>
      <xs:choice>
         <xs:element name="FlightNumberRange"  type="typeFlightNumberRange" minOccurs="0"/>
         <xs:element name="SpecificFlightNumber"  type="typeSpecificFlightNumber" minOccurs="0"/>
      </xs:choice>
   </xs:complexType> 
   
   <xs:complexType name="typeSpecificFlightNumber">
        <xs:annotation>
            <xs:documentation>
                Specify exact flight number.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="FlightNumber" type="common:typeFlightNumber"
            use="required" />       
    </xs:complexType>
        
    <xs:complexType name="typeFlightNumberRange">
        <xs:annotation>
            <xs:documentation>
                Specify a range of flight numbers.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="FlightNumberRangeStart" type="common:typeFlightNumber"
            use="required"/>
            
        <xs:attribute name="FlightNumberRangeEnd" type="common:typeFlightNumber"
            use="required" />
    </xs:complexType>   
   
    <xs:simpleType name="typeClassOfService">
        <xs:annotation>
            <xs:documentation>Class of service code (Booking code) usually one letter, rarely two.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>    
    
    <xs:element name="VehicleUpsellQualify">
        <xs:annotation>
            <xs:documentation>Qualify data of Vehicle against which Vehicle Availability search is matched to get an upsell Offer. Each qualify should have an offer.</xs:documentation>
        </xs:annotation>        
        <xs:complexType>
            <xs:attribute name="Key" type="common:typeRef" use="optional"/>
            <xs:attribute name="VendorCode" type="common:typeSupplierCode" use="required"/>
            <xs:attribute name="EffectiveDate" type="common:typeDate" use="required"/>
            <xs:attribute name="ExpirationDate" type="common:typeDate" use="required"/>
            <xs:attribute name="ProviderCode" type="common:typeProviderCode" use="optional"/>
            <xs:attribute name="PickupDateTime" type="xs:string" use="optional"/>
            <xs:attribute name="PickupLocation" type="common:typeIATACode" use="optional"/>
            <xs:attribute name="ReturnDateTime" type="xs:string" use="optional"/>
            <xs:attribute name="ReturnLocation" type="common:typeIATACode" use="optional"/>
            <xs:attribute name="PickupLocationType" type="common:typeVehicleLocation" use="optional"/>
            <xs:attribute name="ReturnLocationType" type="common:typeVehicleLocation" use="optional"/>
            <xs:attribute name="PickupLocationNumber" type="xs:string" use="optional"/>
            <xs:attribute name="ReturnLocationNumber" type="xs:string" use="optional"/>
            <xs:attribute name="AirConditioning" type="xs:boolean" use="optional"/>
            <xs:attribute name="TransmissionType" type="common:typeVehicleTransmission" use="optional"/>
            <xs:attribute name="VehicleClass" type="common:typeVehicleClass" use="optional"/>
            <xs:attribute name="Category" type="common:typeVehicleCategory" use="optional"/>
            <xs:attribute name="DoorCount" type="common:typeDoorCount" use="optional"/>
            <xs:attribute name="RateCode" type="common:typeRateCode" use="optional"/>
            <xs:attribute name="RateCategory" type="common:typeRateCategory" use="optional"/>
            <xs:attribute name="DiscountNumber" type="common:typeDiscountNumber" use="optional"/>
            <xs:attribute name="OfferRef" type="common:typeRef" use="optional"/>
            <xs:attributeGroup ref="common:attrElementKeyResults"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="VehicleUpsellOffer">
        <xs:annotation>
            <xs:documentation>Offer data of Vehicle which is used to send an upsell request. Offer is found for a matching qualify.</xs:documentation>  
        </xs:annotation>        
        <xs:complexType>
            <xs:attribute name="AirConditioning" type="xs:boolean" use="required"/>
            <xs:attribute name="TransmissionType" type="common:typeVehicleTransmission" use="required"/>
            <xs:attribute name="VehicleClass" type="common:typeVehicleClass" use="required">
                <xs:annotation>
                    <xs:documentation>Class of vehicle</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Category" type="common:typeVehicleCategory" use="required">
                <xs:annotation>
                    <xs:documentation>Category of vehicle. Each supplier decides how these categories map to a vehicle class.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DoorCount" type="common:typeDoorCount" use="optional">
                <xs:annotation>
                    <xs:documentation>The number of doors on the vehicle. Could be a range like '2-4'</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="RateCode" type="common:typeRateCode" use="optional"/>
            <xs:attribute name="RateCategory" type="common:typeRateCategory" use="optional">
                <xs:annotation>
                    <xs:documentation>The category of this rate (Best, etc)</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DiscountNumber" type="common:typeDiscountNumber" use="optional"/>
            <xs:attribute name="FuelType" type="common:typeFuel" use="optional"/>
            <xs:attribute name="Key" type="common:typeRef" use="optional"/>
            <xs:attributeGroup ref="common:attrElementKeyResults"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="HotelUpsellQualify">
        <xs:annotation>
            <xs:documentation>Qualify data of Hotel against which Hotel property details search is matched to get an upsell Offer. Each qualify should have an offer.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="common:CorporateDiscountID" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
            <xs:attribute name="HotelChainCode" type="common:typeHotelChainCode" use="required"/>
            <xs:attribute name="HotelCode" type="common:typeHotelCode" use="optional"/>
            <xs:attribute name="HotelLocation" type="common:typeIATACode" use="optional">
                <xs:annotation>
                    <xs:documentation>The IATA location code for this entity.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="RatePlanType" type="common:typeRatePlanType" use="optional"/>
            <xs:attribute name="EffectiveDate" type="common:typeDate" use="required"/>
            <xs:attribute name="ExpirationDate" type="common:typeDate" use="required"/>
            <xs:attribute name="Key" type="common:typeRef" use="optional"/>
            <xs:attributeGroup ref="common:attrElementKeyResults"/>
            <xs:attribute name="OfferRef" type="common:typeRef" use="optional">
                <xs:annotation>
                    <xs:documentation>Reference to the Offer.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:element name="HotelUpsellOffer">
        <xs:annotation>
            <xs:documentation>Offer data of Hotel which is used to send a Hotel Upsell request and match a hotel property in the response. Offer is found for a matching qualify.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="common:CorporateDiscountID" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
            <xs:attribute name="RatePlanType" type="common:typeRatePlanType" use="required"/>
            <xs:attribute name="Key" type="common:typeRef" use="optional"/>
            <xs:attributeGroup ref="common:attrElementKeyResults"/>
        </xs:complexType>
    </xs:element>       

