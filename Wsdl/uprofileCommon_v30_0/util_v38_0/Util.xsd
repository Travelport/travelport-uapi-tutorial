<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.travelport.com/schema/util_v38_0"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    targetNamespace="http://www.travelport.com/schema/util_v38_0"
    xmlns:air="http://www.travelport.com/schema/air_v38_0"
    xmlns:common="http://www.travelport.com/schema/common_v38_0"
    elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:annotation>
        <xs:documentation>
            (c) Travelport GDS 2008 All rights reserved. Proprietary
            Information of Travelport GDS.
        </xs:documentation>
    </xs:annotation>

    <xs:import schemaLocation="../common_v38_0/CommonReqRsp.xsd"
        namespace="http://www.travelport.com/schema/common_v38_0" />
    <xs:import namespace="http://www.travelport.com/schema/common_v38_0"
        schemaLocation="../common_v38_0/Common.xsd" />
    <xs:import namespace="http://www.travelport.com/schema/air_v38_0"
        schemaLocation="../air_v38_0/Air.xsd" />

    <!-- Upsell Req/Rsp Start-->

    <xs:element name="UpsellAdminReq">
        <xs:annotation>
            <xs:documentation>
                Request to add/delete/update qualify/offer.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:choice>
                        <xs:element ref="AirUpsellCriteria" minOccurs="0" maxOccurs="1"/>
                        <xs:element ref="VehicleUpsellCriteria" minOccurs="0" maxOccurs="1"/>
                        <xs:element ref="HotelUpsellCriteria" minOccurs="0"  maxOccurs="1"/>                                                    
                    </xs:choice>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="UpsellAdminRsp">
        <xs:annotation>
            <xs:documentation>
                Response to add/delete/update of offer/qualify.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:choice>
                        <xs:element ref="AirUpsellRule" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="VehicleUpsellRule" minOccurs="0" maxOccurs="unbounded"/>
                        <xs:element ref="HotelUpsellRule" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:choice>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="UpsellSearchReq">
        <xs:annotation>
            <xs:documentation>Request to retrieve all upsell qualify/offers based on search criteria for air,hotel and vehicle.
            </xs:documentation>
            </xs:annotation>
            <xs:complexType>
                <xs:complexContent>
                    <xs:extension base="common:BaseReq">
                        <xs:sequence>
                            <xs:choice>
                                <xs:element ref="AirUpsellSearchCriteria" minOccurs="0"/>
                                <xs:element ref="HotelUpsellSearchCriteria" minOccurs="0"/>
                                <xs:element ref="VehicleUpsellSearchCriteria" minOccurs="0"/>
                            </xs:choice>
                            <xs:element ref="UpsellSearchModifier" maxOccurs="1"/>
                        </xs:sequence>
                    </xs:extension>
                </xs:complexContent>
            </xs:complexType>
    </xs:element>
     <xs:element name="UpsellSearchRsp">
         <xs:annotation>
             <xs:documentation>
                         Response containing qualify and offer for the matching search criteria.
             </xs:documentation>
         </xs:annotation>
         <xs:complexType>
             <xs:complexContent>
                 <xs:extension base="common:BaseRsp">
                      <xs:choice>
                          <xs:element ref="AirUpsellSearchResult" minOccurs="0" maxOccurs="unbounded"/>
                          <xs:element ref="HotelUpsellSearchResult" minOccurs="0" maxOccurs="unbounded"/>
                          <xs:element ref="VehicleUpsellSearchResult" minOccurs="0" maxOccurs="unbounded"/>
                      </xs:choice>
                      <xs:attribute name="MoreResults" type="common:typeMoreResults" use="required"/>
                 </xs:extension>
             </xs:complexContent>
       </xs:complexType>
    </xs:element>
    
    <!-- Upsell Req/Rsp Ends -->

    <!--*** Request Types ***-->
    <xs:element name="CreditCardAuthReq">
        <xs:annotation>
            <xs:documentation>
                Performs a credit card authorization to validate a
                credit card for use during booking
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="common:LocatorCode"
                            minOccurs="0" />
                        <xs:element ref="CreditCardPaymentAuth"
                            maxOccurs="unbounded" />
                    </xs:sequence>
                    <xs:attribute name="PlatingCarrier"
                        type="common:typeCarrier" use="required" />
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="CreditCardAuthRsp">
        <xs:annotation>
            <xs:documentation>
                The response to the CreditCardAuthReq.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="common:CreditCardAuth"
                            maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="CurrencyConversionReq">
        <xs:annotation>
            <xs:documentation>
                Performs A Currency Conversion between two Currencies
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="CurrencyConversion"
                            minOccurs="1" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="CurrencyConversionRsp">
        <xs:annotation>
            <xs:documentation>
                The response to the CurrencyConversionReq.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="CurrencyConversion"
                            minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="CreateAgencyFeeMcoReq">
        <xs:annotation>
            <xs:documentation>
                Manually create an Agency Fee MCO.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="common:Name" />
                        <xs:choice>
                            <xs:element ref="common:FormOfPayment"
                                minOccurs="0" />
                            <xs:element ref="common:FormOfPaymentRef"
                                minOccurs="0" />
                        </xs:choice>
                        <xs:element ref="common:GeneralRemark"
                            minOccurs="0" maxOccurs="unbounded" />
                        <xs:element ref="common:AccountingRemark"
                            minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                    <xs:attribute name="Amount" type="common:typeMoney"
                        use="required" />
                    <xs:attribute name="LocationCode"
                        type="common:typeAirport" use="optional">
                        <xs:annotation>
                            <xs:documentation>
                                The airport code where the MCO will be
                                accepted, or the City code where the
                                requesting agency resides. If not
                                entered, the default location of the
                                agency will be selected.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="LocatorCode" use="optional">
                        <xs:annotation>
                            <xs:documentation>
                                The locator code of the PNR that the MCO
                                is created for.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="8" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="TicketNumber" type="xs:string"
                        use="optional">
                        <xs:annotation>
                            <xs:documentation>
                                The ticket that this MCO was issued in
                                connection with. Could be the ticket
                                that caused the fee, a residual from an
                                exchange, or an airline service fee.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
   	<xs:element name="CreateAgencyFeeMcoRsp">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="common:MCO" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <!-- there is not binding for this element -->
    <xs:element name="CreateAirlineFeeMcoReq">
        <xs:annotation>
            <xs:documentation>
                Manually create an Airline Fee MCO.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="common:Name" />
                        <xs:choice>
                            <xs:element ref="common:FormOfPayment"
                                minOccurs="0" />
                            <xs:element ref="common:FormOfPaymentRef"
                                minOccurs="0" />
                        </xs:choice>
                        <xs:element ref="common:GeneralRemark"
                            minOccurs="0" maxOccurs="unbounded" />
                        <xs:element ref="common:AccountingRemark"
                            minOccurs="0" maxOccurs="unbounded" />
                        <xs:element ref="common:Endorsement"
                            minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                    <xs:attribute name="Amount" type="common:typeMoney"
                        use="required" />
                    <xs:attribute name="LocationCode"
                        type="common:typeAirport" use="required">
                        <xs:annotation>
                            <xs:documentation>
                                The airport code where the MCO will be
                                accepted, or the City code where the
                                requesting agency resides. If not
                                entered, the default location of the
                                agency will be selected.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="LocatorCode" use="required">
                        <xs:annotation>
                            <xs:documentation>
                                The locator code of the PNR that the MCO
                                is created for.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="8" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="TicketNumber" type="xs:string"
                        use="optional">
                        <xs:annotation>
                            <xs:documentation>
                                The ticket that this MCO was issued in
                                connection with. Could be the ticket
                                that caused the fee, a residual from an
                                exchange, or an airline service fee.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="CreateAirlineFeeMcoRsp">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="common:MCO" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="MirReportRetrieveReq">
        <xs:annotation>
            <xs:documentation>Retrieve a report</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:attribute name="LocatorCode" use="required">
                        <xs:annotation>
                            <xs:documentation>
                                The locator code of the PNR that the MIR
                                is created for.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="8" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                    <xs:attribute name="ReportFormat" use="optional">
                        <xs:annotation>
                            <xs:documentation>
                                MIR report format type
                            </xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="TRAMS" />
                                <xs:enumeration value="CLIENTBASE" />
                                <xs:enumeration value="TRISEPT" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:attribute>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="MirReportRetrieveRsp">
        <xs:annotation>
            <xs:documentation>
                Carries the report payload
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:annotation>
                            <xs:documentation>
                                The text of the MIR report.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:element name="Report" type="xs:string" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="McoSearchReq">
        <xs:annotation>
            <xs:documentation>
                Search for MCOs by certain criteria and return a list.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="common:Name" minOccurs="0" />
                        <xs:element ref="common:Carrier" minOccurs="0"
                            maxOccurs="10" />
                        <xs:element ref="common:Airport" minOccurs="0"
                            maxOccurs="10" />
                        <xs:element ref="common:TicketNumber"
                            minOccurs="0">
                            <xs:annotation>
                                <xs:documentation>
                                    The ticket that this MCO was issued
                                    in connection with. Could be the
                                    ticket that caused the fee, a
                                    residual from an exchange, or an
                                    airline service fee.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element ref="McoCreateDate" minOccurs="0" />
                        <xs:element ref="McoSearchModifiers"
                            minOccurs="0" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="McoSearchRsp">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="McoSearchResult" minOccurs="0"
                            maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="McoVoidReq">
        <xs:annotation>
            <xs:documentation>
                Void an MCO (of any type).
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="common:GeneralRemark"
                            minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                    <xs:attribute name="Number" type="xs:string"
                        use="required">
                        <xs:annotation>
                            <xs:documentation>
                                The number of the MCO to void
                            </xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="ReturnMCO" type="xs:boolean"
                        use="optional" default="false" />
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="McoVoidRsp">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="common:MCO" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="MctLookupReq">
        <xs:annotation>
            <xs:documentation>
                Search for MCT time values
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:choice>
                        <xs:element ref="MctSearch" minOccurs="0" />
                        <xs:element ref="MctQuery" minOccurs="0" />
                    </xs:choice>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="MctLookupRsp">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="MctStandard" minOccurs="0"
                            maxOccurs="4" />
                        <xs:element ref="MctException" minOccurs="0"
                            maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="MctCountReq">
        <xs:annotation>
            <xs:documentation>
                Determine how many MCT exceptions exist for the search
                criteria
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="MctSearch" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="MctCountRsp">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="MctCount" minOccurs="0"
                            maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="ReferenceDataRetrieveReq">
        <xs:annotation>
            <xs:documentation>
                Request to retrieve code, name and description for  a specific reference data type.
             </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                         <xs:element ref="ReferenceDataSearchModifiers" minOccurs="0" />     
                         <xs:element ref="RequestReferenceDataItem" minOccurs="0" maxOccurs="1"/>                                   
                    </xs:sequence>
                    <xs:attribute name="TypeCode" type="common:typeTypeCode" use="required">
                        <xs:annotation>
                            <xs:documentation>The code of the reference data type. Valid values are 'PaymentType', 'PaymentFormatType', 'MerchandisingOfferType', 'HotelTaxType', 'HotelAmenities','Airport','City','Country','CityAirport','BusinessType','Currency','PosChannel','StateProvince','SupplierType','RoleCategoryType','ResourceCategoryType','PaymentType','EmailType','HotelRateCategory','FulfillmentType','HotelRoomViewType', 'HotelMealPlans' , 'GeoPoliticalAreaType', 'AirAndRailMiscType', 'AirAndRailSupplierType', 'HotelMiscType' , 'HotelSupplierType' , 'VehicleMiscType' , 'VehicleSupplierType' , 'AccountingReferenceType' , 'TRMLocation' , 'Title' , 'PassengerTypeCode' , 'Gender' , 'PostalAddressType' , 'AccountingRemarkType' , 'VehicleSpecialEquipment', 'ReferencePointSearch', 'RailStationLocation', 'RailAccommodation', and 'RailDiscountLoyalty'.</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>                 
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="RailStationLocationModifiers">
	<xs:annotation>
		<xs:documentation>Request object used to request specific rail station
			locations.</xs:documentation>
	</xs:annotation>
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="common:RailLocation" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="CountryCode" type="common:typeCountry" use="optional">
			<xs:annotation>
				<xs:documentation>2 character country code such as FR.
				</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Distributor" type="common:typeSupplierCode" use="optional">
			<xs:annotation>
				<xs:documentation>2 character Rail distributor code such as TL or
					2C.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>A city name or station name such as Paris or Paris
					Nord.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Active" type="xs:boolean" use="optional"
			default="true">
			<xs:annotation>
				<xs:documentation>The value “false” will return stations that are no
					longer active but remain on the table because existing bookings may
					reference them. The default is “true” which returns only active
					locations.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
</xs:element>
            
    <xs:element name="RequestReferenceDataItem">
      <xs:annotation>
         <xs:documentation>
                Limits the responses to the requested subcategories for a specific Reference Data Type, Currently supported only for @TypeCode="HotelAmenities".
            </xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element name="RequestAmenity" minOccurs="0" maxOccurs="9">
               <xs:annotation>
                  <xs:documentation>
                         Requested decoded values only for the specified Hotel Amenity codes.
                     </xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:attribute name="Type" use="required">
                     <xs:annotation>
                        <xs:documentation>Limits to the response to the requested amenity type. Can be a general type: “HA” (Hotel Property Amenity) or “RA” (Room Amenity). Or, a specific type, for example: “TR” (Transportation) or “BT” (Bed Type). If no type is specified, all amenity types are returned.</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:length value="2"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:attribute>
               </xs:complexType>
            </xs:element>
         </xs:sequence>
      </xs:complexType>
   </xs:element>

    <xs:element name="ReferenceDataRetrieveRsp">
        <xs:annotation>
            <xs:documentation>
                Response to retrieve code, name and description for a specific reference data type. 
                </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:sequence>
                        <xs:element ref="ReferenceDataItem" minOccurs="0" maxOccurs="unbounded" />
                    </xs:sequence>
                    <xs:attribute name="MoreResults" type="common:typeMoreResults" use="required"/>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="ReferenceDataSearchReq">
        <xs:annotation>
            <xs:documentation>
                Request to lookup a specific reference data item
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseReq">
                    <xs:sequence>
                        <xs:element ref="ReferenceDataSearchModifiers"
                            minOccurs="0" />
                        <xs:element ref="ReferenceDataSearchItem"
                            minOccurs="1" maxOccurs="unbounded" />
                    </xs:sequence>
                </xs:extension>
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="ReferenceDataSearchRsp">
        <xs:annotation>
            <xs:documentation>
                Response the sought reference data item
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="common:BaseRsp">
                    <xs:choice>
                        <xs:element name="Airport" minOccurs="0"
                            maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="Code"
                                    type="common:typeAirport" use="required" />
                                <xs:attribute name="Name"
                                    type="xs:string" use="optional" />
                                <xs:attribute name="CityCode"
                                    type="common:typeIATACode" use="optional" />
                                <xs:attribute name="CountryCode"
                                    type="common:typeCountry" use="optional" />
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="Carrier" minOccurs="0"
                            maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:attribute name="Code"
                                    type="common:typeCarrier" use="required" />
                                <xs:attribute name="Name"
                                    type="xs:string" use="optional" />
                                <xs:attribute name="Type"
                                    type="xs:string" use="optional" />
                            </xs:complexType>
