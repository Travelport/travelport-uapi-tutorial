							<xs:documentation>If entered, can be a part of the descirption</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ExecuteDate" type="xs:date"/>
					<xs:attribute name="Published" type="xs:boolean"/>
					<xs:attribute name="Active" type="xs:boolean"/>
					<xs:attribute name="Immutable" type="xs:boolean"/>
					<xs:attribute name="SystemOwned" type="xs:boolean"/>
					<xs:attribute name="UnAssigned" type="xs:boolean">
